{
  "UILogo": "pickstock.png",
  "miroLogFile": "miro.log",
  "scripts": {
    "hcube": [
      {
        "title": "High level scalar analysis",
        "id": "script1",
        "command": "jupyter",
        "args": ["nbconvert", "--to", "html", "--execute", "hcube_analysis.ipynb"],
        "outputFile": "hcube_analysis.html"
      }
    ]
  },
  "inputWidgets": {
    "price": {
      "widgetType": "table",
      "readonlyCols": "date"
    },
    "maxstock": {
      "label": "select the maximum number of stocks",
      "widgetType": "slider",
      "alias": "maximum number of stocks to select",
      "min": 1,
      "max": "card(price$symbol)",
      "default": 3,
      "step": 1,
      "ticks": false,
      "noHcube": false
    },
    "trainingdays": {
      "label": "select the number of days for training",
      "min": 1,
      "default": 99,
      "step": 1,
      "max": "card(price$date)",
      "alias": "number of days for training",
      "widgetType": "slider"
    },
    "solver": {
      "alias": "MIP-Solver",
      "widgetType": "dropdown",
      "label": "Solver to use",
      "choices": ["BARON", "BDMLP", "CBC", "CPLEX", "LINDO", "LINDOGLOBAL", "MOSEK", "SCIP", "XPRESS"],
      "selected": "CPLEX"
    }
  },
  "dataRendering": {
    "stock_weight": {
      "outType": "dtGraph",
      "graph": {
        "title": "Part of portfolio",
        "tool": "plotly",
        "type": "pie",
        "traces": [
          {
            "labels": "symbol",
            "values": "value",
            "hole": 0,
            "name": "weight"
          }
        ],
        "showlegend": true
      },
      "height": 700
    },
    "dowvsindex": {
      "outType": "graph",
      "height": 700,
      "graph": {
        "tool": "dygraphs",
        "title": "Dow Jones vs. Index Fund",
        "dyOptions": {
          "includeZero": false,
          "logscale": false,
          "drawGrid": true,
          "drawPoints": false,
          "pointSize": 2,
          "fillGraph": false,
          "fillAlpha": 0.15
        },
        "dyHighlight": {
          "highlightSeriesOpts": {
            "strokeWidth": 3,
            "strokeBorderWidth": 1,
            "highlightCircleSize": 5
          },
          "hideOnMouseOut": true
        },
        "dyLegend": {
          "width": 450,
          "show": "always",
          "hideOnMouseOut": true
        },
        "dyRangeSelector": {
          "height": 20,
          "strokeColor": ""
        },
        "dyEvent": {
          "lastdaytraining": {
            "label": "End of training phase",
            "labelLoc": "bottom",
            "color": "red"
          }
        },
        "xdata": "date",
        "ydata": {
          "dj": {
            "label": "Dow Jones index",
            "stepPlot": false,
            "stemPlot": false,
            "fillGraph": false,
            "drawpoints": true
          },
          "index fund": {
            "label": "Index fund",
            "stepPlot": false,
            "stemPlot": false,
            "fillGraph": false,
            "drawpoints": true
          }
        },
        "xaxis": {
          "name": "x",
          "label": "Date"
        },
        "yaxis": {
          "name": "y",
          "label": "Price"
        }
      }
    },
    "abserror": {
      "outType": "dtGraph",
      "height": 700,
      "graph": {
        "tool": "plotly",
        "type": "hist",
        "histnorm": "percent",
        "nbins": 20,
        "barmode": "overlay",
        "alpha": 0.6,
        "title": "Absolute Error: Dow Jones vs. Index Fund",
        "xdata": {
          "absolute error train": {
            "labels": "training",
            "color": "darkgreen",
            "alpha": 0.6
          },
          "absolute error test": {
            "labels": "test",
            "color": "red",
            "alpha": 0.6
          }
        },
        "xaxis": {
          "title": "Absolute Error"
        },
        "yaxis": {
          "title": "Frequency"
        }
      }
    },
    "pricemerge": {
      "outType": "graph",
      "graph": {
        "tool": "dygraphs",
        "xdata": "date",
        "title": "Price (stocks & dow jones)",
        "dyOptions": {
          "logscale": false,
          "stepPlot": false,
          "stemPlot": false,
          "fillGraph": false,
          "fillAlpha": 0.15,
          "drawPoints": false,
          "pointShape": "dot",
          "pointSize": 2
        },
        "xaxis": {
          "name": "x",
          "label": "Date",
          "axisLineWidth": 0.3,
          "axisLabelFontSize": 14,
          "gridLineWidth": 0.3,
          "drawGrid": true
        },
        "yaxis": {
          "name": "y",
          "label": "Price",
          "axisLineWidth": 0.3,
          "axisLabelFontSize": 14,
          "gridLineWidth": 0.3,
          "gridLineColor": "gray",
          "valueRange": [null, null],
          "independentTicks": true
        },
        "yaxis2": {
          "name": "y2",
          "label": "Price (stocks & dow jones)",
          "axisLineWidth": 0.3,
          "axisLabelFontSize": 14,
          "gridLineWidth": 0.3,
          "drawGrid": true,
          "valueRange": [null, null],
          "independentTicks": true
        },
        "ydata": {
          "value": {
            "label": "Price (stocks & dow jones)",
            "stemPlot": false,
            "stepPlot": false,
            "fillGraph": false,
            "drawPoints": false,
            "pointShape": "dot",
            "pointSize": 2,
            "yaxis": "y"
          }
        },
        "dyHighlight": {
          "hideOnMouseOut": true,
          "highlightCircleSize": 0,
          "highlightSeriesBackgroundAlpha": 1,
          "highlightSeriesOpts": {
            "strokeWidth": 0
          }
        },
        "color": "uni",
        "filter": {
          "col": "uni",
          "label": "Select stock symbol",
          "multiple": true,
          "date": false
        },
        "dyEvent": {
          "lastdaytraining": {
            "labelLoc": "top",
            "strokePattern": "dashed",
            "label": "End of training phase",
            "color": "#ff0000"
          }
        },
        "dyRangeSelector": {
          "height": 25,
          "strokeColor": "#808fab",
          "fillColor": "#a7b1c4",
          "retainDateWindow": false,
          "keepMouseZoom": true
        }
      },
      "height": 700
    },
    "price": {
      "outType": "graph",
      "graph": {
        "tool": "dygraphs",
        "xdata": "date",
        "ydata": {
          "value": {
            "label": "Price",
            "stemPlot": false,
            "stepPlot": false,
            "fillGraph": false,
            "drawPoints": false,
            "pointShape": "dot",
            "pointSize": 2,
            "yaxis": "y"
          }
        },
        "title": "Stock prices over time (year: 2016)",
        "dyOptions": {
          "logscale": false,
          "stepPlot": false,
          "stemPlot": false,
          "fillGraph": false,
          "fillAlpha": 0.15,
          "drawPoints": false,
          "pointShape": "dot",
          "pointSize": 2
        },
        "dyHighlight": {
          "highlightCircleSize": 3,
          "highlightSeriesBackgroundAlpha": 0.8,
          "hideOnMouseOut": true,
          "highlightSeriesOpts": {
            "strokeWidth": 3,
            "strokeBorderWidth": null,
            "strokeBorderColor": "#ffffff"
          }
        },
        "dyLegend": {
          "show": "auto",
          "width": 250,
          "showZeroValues": false,
          "labelsSeparateLines": false
        },
        "dyRangeSelector": {
          "height": 25,
          "strokeColor": "#808fab",
          "fillColor": "#a7b1c4",
          "retainDateWindow": false,
          "keepMouseZoom": true
        },
        "color": "symbol",
        "xaxis": {
          "name": "x",
          "label": "Date",
          "axisLineWidth": 0.3,
          "axisLabelFontSize": 14,
          "gridLineWidth": 0.3,
          "drawGrid": true
        },
        "yaxis": {
          "name": "y",
          "label": "Price",
          "axisLineWidth": 0.3,
          "axisLabelFontSize": 14,
          "gridLineWidth": 0.3,
          "gridLineColor": "gray",
          "valueRange": [null, null],
          "independentTicks": true
        },
        "yaxis2": {
          "name": "y2",
          "label": "Price",
          "axisLineWidth": 0.3,
          "axisLabelFontSize": 14,
          "gridLineWidth": 0.3,
          "drawGrid": true,
          "valueRange": [null, null],
          "independentTicks": true
        }
      },
      "height": 700
    }
  },
  "storeLogFilesDuration": 0,
  "roundingDecimals": 0,
  "activateModules": {
    "loadLocal": false,
    "logFile": false,
    "lstFile": false,
    "attachments": false,
    "remoteExecution": false,
    "downloadTempFiles": false
  },
  "aggregateWidgets": false,
  "excelIncludeMeta": false,
  "excelIncludeEmptySheets": false,
  "saveTraceFile": false,
  "autoGenInputGraphs": false,
  "theme": "dark",
  "defCompMode": "tab",
  "symbolLinks": [
    {
      "source": "pricemerge",
      "target": "price"
    }
  ],
  "overwriteSheetOrder": {
    "input": ["_widgets", "price", "_scalars"],
    "output": ["dowvsindex", "stock_weight", "abserror", "pricemerge", "_scalars_out"]
  },
  "pageTitle": "Test title",
  "defaultScenName": "defaultTest",
  "hiddenOutputScalars": ["firstdaytraining", "lastdaytraining"],
  "overwriteAliases": {
    "price": {
      "newAlias": "Symbol alias for price"
    },
    "_scalars_out": {
      "newAlias": "Output Scalars"
    },
    "stock_weight": {
      "newAlias": "weight"
    },
    "dowvsindex": {
      "newAlias": "dow jones vs. index fund"
    },
    "abserror": {
      "newAlias": "absolute error"
    },
    "pricemerge": {
      "newAlias": "Price (stocks & dow jones)"
    }
  },
  "overwriteHeaderAliases": {
    "price": {
      "newHeaders": ["1st header", "2nd header", "3rd header"]
    },
    "_scalars_out": {
      "newHeaders": ["Scalar Name", "Scalar Description", "Value"]
    }
  },
  "readme": {
    "tabTitle": "Readme",
    "filename": "Readme.md",
    "enableMath": true
  },
  "outputAttachments": [
    {
      "filename": "testOutputAttachment",
      "execPerm": true,
      "throwError": false
    },
    {
      "filename": "testOutputAttachment2",
      "execPerm": false,
      "throwError": true
    }
  ],
  "extraClArgs": "--test=true",
  "outputGroups": [
    {
      "name": "Dow Jones vs. Index Fund ⨠",
      "members": ["dowvsindex", "abserror"]
    }
  ],
  "handsontable": {
    "stretchH": "none",
    "readonly": false,
    "highlightCol": false,
    "highlightRow": false,
    "columnSorting": false,
    "manualColumnResize": false,
    "contextMenu": {
      "enabled": true,
      "allowRowEdit": false,
      "allowColEdit": false,
      "allowReadOnly": true
    },
    "height": 250,
    "colWidths": 200
  },
  "datatable": {
    "class": "stripe",
    "filter": "top",
    "rownames": true,
    "options": {
      "pageLength": 5,
      "buttons": ["copy", "csv", "pdf"],
      "dom": "Bfrtip"
    },
    "extensions": "Buttons"
  }
}
