{
  "pivottable": {
    "bgColor": "rgb(255,128,0)"
  },
  "defCompMode": "split",
  "storeLogFilesDuration": 7,
  "roundingDecimals": 2,
  "activateModules": {
    "loadLocal": true,
    "logFile": true,
    "lstFile": true,
    "attachments": true,
    "remoteExecution": false,
    "downloadTempFiles": false
  },
  "aggregateWidgets": true,
  "excelIncludeMeta": true,
  "excelIncludeEmptySheets": true,
  "saveTraceFile": true,
  "autoGenInputGraphs": true,
  "overwriteSheetOrder": {
    "input": "_widgets"
  },
  "theme": "browser",
  "handsontable": {
    "stretchH": "none",
    "readonly": false,
    "highlightCol": true,
    "highlightRow": true,
    "columnSorting": true,
    "manualColumnResize": true,
    "contextMenu": {
      "enabled": true,
      "allowRowEdit": true,
      "allowColEdit": true,
      "allowReadOnly": false,
      "allowComments": true
    },
    "height": 700,
    "colWidths": 150
  }
}
