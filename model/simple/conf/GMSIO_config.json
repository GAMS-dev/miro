{
    "pageTitle": "Simplified Energy Systems Model",
    "gamsMetaDelim": "###",
    "gamsWEBUISwitch": "--GMSWEBUI=1",
    "fileExchange": "csv",
    "csvDelim": ",",
    "gamsInputFiles": {
        "planttypedata": {
            "alias": "Plant information",
            "headers": {
                "plant type": {
                    "type": "set"
                },
                "co2": {
                    "type": "parameter"
                },
                "cost": {
                    "type": "parameter"
                },
                "expansion cost": {
                    "type": "parameter"
                }
            }
        },
        "timeseries": {
            "alias": "base case time series [GWh]",
            "headers": {
                "Time Intervals": {
                    "type": "set"
                },
                "photovoltaic": {
                    "type": "parameter"
                },
                "wind": {
                    "type": "parameter"
                },
                "demand": {
                    "type": "parameter"
                }
            }
        }
    },
    "gamsOutputFiles": {
        "rep_emix": {
            "alias": "energy mix report",
            "headers": {
                "time steps": {
                    "type": "set"
                },
                "Regions": {
                    "type": "set"
                },
                "lng": {
                    "type": "parameter"
                },
                "lat": {
                    "type": "parameter"
                },
                "total": {
                    "type": "parameter"
                },
                "renewable": {
                    "type": "parameter"
                },
                "fossil": {
                    "type": "parameter"
                }
            }
        },
        "rep_flow": {
            "alias": "flow report",
            "headers": {
                "time steps": {
                    "type": "set"
                },
                "Regions": {
                    "type": "set"
                },
                "lng0": {
                    "type": "parameter"
                },
                "lat0": {
                    "type": "parameter"
                },
                "lng1": {
                    "type": "parameter"
                },
                "lat1": {
                    "type": "parameter"
                },
                "flow": {
                    "type": "parameter"
                }
            }
        },
        "scalars_out": {
            "alias": "Scalars",
            "count": 3,
            "headers": {
                "Scalar": {
                    "type": "set"
                },
                "Description": {
                    "type": "acronym"
                },
                "Value": {
                    "type": "acronym"
                }
            }
        }
    }
}