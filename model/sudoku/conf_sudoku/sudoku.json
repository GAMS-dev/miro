{
  "overwriteSheetOrder": {
    "input": ["initial_state", "_widgets"]
  },
  "pivottable": {
    "bgColor": "rgb(255,128,0)"
  },
  "theme": "browser",
  "defCompMode": "split",
  "roundingDecimals": 2,
  "activateModules": {
    "logFile": false,
    "lstFile": false,
    "loadLocal": true,
    "attachments": true,
    "downloadTempFiles": false
  },
  "autoGenInputGraphs": true,
  "aggregateWidgets": true,
  "storeLogFilesDuration": 7,
  "excelIncludeMeta": true,
  "excelIncludeEmptySheets": true,
  "saveTraceFile": true,
  "dataRendering": {
    "results": {
      "outType": "sudoku"
    },
    "initial_state": {
      "outType": "sudoku"
    }
  },
  "inputWidgets": {
    "initial_state": {
      "widgetType": "custom",
      "rendererName": "sudoku",
      "alias": "Initial state",
      "options": {
        "isInput": true
      }
    },
    "force_unique_sol": {
      "widgetType": "checkbox",
      "alias": "Force unique solution",
      "value": 1,
      "noHcube": false,
      "class": "checkbox-material",
      "label": "Compute only if solution is unique?"
    }
  },
  "pageTitle": "Sudoku solver",
  "miroLogFile": "miro.log",
  "handsontable": {
    "stretchH": "none",
    "readonly": false,
    "highlightCol": true,
    "highlightRow": true,
    "columnSorting": true,
    "manualColumnResize": true,
    "contextMenu": {
      "enabled": true,
      "allowRowEdit": true,
      "allowColEdit": true,
      "allowReadOnly": false,
      "allowComments": true
    },
    "height": 700,
    "colWidths": 150
  }
}
