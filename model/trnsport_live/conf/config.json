{
    "inputWidgets":{
       "f":{
          "widgetType": "slider",
          "alias": "freight in dollars per case per thousand miles",
          "label": "freight in dollars per case per thousand miles",
          "min": 1,
          "max": 500,
          "default": 90,
          "step": 1
       },
       "mins":{
          "widgetType": "slider",
          "alias": "minimum shipment (MIP- and MINLP-only)",
          "label": "minimum shipment (MIP- and MINLP-only)",
          "min": 0,
          "max": 500,
          "default": 100,
          "step": 1
       },
       "beta":{
          "widgetType": "slider",
          "alias": "beta (MINLP-only)",
          "label": "beta (MINLP-only)",
          "min": 0,
          "max": 1,
          "default": 0.95,
          "step": 0.01
       },
       "GMSPAR_type":{
          "widgetType": "dropdown",
          "alias": "Model Type",
          "label": "Select model type",
          "aliases": [
              "LP",
              "MIP",
              "MINLP"
          ],
          "choices": [
              "lp",
              "mip",
              "minlp"
          ]
       },
       "GMSOPT_lp":{
          "widgetType": "textinput",
          "label": "Select solver",
          "placeholder":"CPLEX"
       }
    },
    "dataRendering"      : {
        "schedule"			: {
		    "outType"			: "trnsport1",
		    "height"			: 700,
		    "packages"			: ["leaflet", "leaflet.minicharts", "sf", "RColorBrewer"],
		    "options"			: {
		    	"geojsonloc"		: "us-states.geojson"
		    }
	    },
        "scalars_out"   : {
            "outType"			: "valueBox",
            "options"  :{
                "icon":{
                    "name" : "list"
                },
                "color":"aqua"
            }
        }
    }
}
