{
    "pageTitle": "Stock Selection Optimization",
    "gamsMetaDelim": "###",
    "gamsWEBUISwitch": "--GMSWEBUI=1",
    "fileExchange": "csv",
    "csvDelim": ",",
    "gamsInputFiles": {
        "stockdata": {
            "alias": "data of stock on date",
            "headers": {
                "date": {
                    "type": "set",
                    "readonly": true
                },
                "stockSymbol": {
                    "type": "set"
                },
                "price": {
                    "type": "parameter"
                }
            }
        },
        "maxstock": {
            "alias": "maximum number of stocks to select",
            "slider": {
                "label": "maximum number of stocks to select",
                "min": 1,
                "max": "card(stockdata$stocksymbol)",
                "default": 5,
                "step": 1
            }
        },
        "trainingdays": {
            "alias": "number of days for training",
            "slider": {
                "label": "number of days for training",
                "min": 1,
                "max": "card(stockdata$date)",
                "default": 99,
                "step": 1
            }
        }
    },
    "gamsOutputFiles": {
        "partofportfolio": {
            "alias": "what part of the portfolio",
            "headers": {
                "stockSymbol": {
                    "type": "set"
                },
                "weight": {
                    "type": "parameter"
                }
            }
        },
        "dowvsindex": {
            "alias": "dow jones vs. index fund",
            "headers": {
                "date": {
                    "type": "set"
                },
                "dow jones": {
                    "type": "parameter"
                },
                "index fund": {
                    "type": "parameter"
                }
            }
        },
        "abserror": {
            "alias": "absolute error",
            "headers": {
                "date": {
                    "type": "set"
                },
                "absolute error train": {
                    "type": "parameter"
                },
                "absolute error test": {
                    "type": "parameter"
                }
            }
        },
        "scalars_out": {
            "alias": "Scalars",
            "hidden": true,
            "count": 4,
            "headers": {
                "Scalar": {
                    "type": "set"
                },
                "Description": {
                    "type": "acronym"
                },
                "Value": {
                    "type": "acronym"
                }
            }
        }
    }
}