{
    "pageTitle": "Stock Selection Optimization",
    "gamsMetaDelim": "[MIRO:hidden]",
    "MIROSwitch": "--MIRO=RUN",
    "fileExchange": "csv",
    "csvDelim": ",",
    "gamsInputFiles": {
        "price": {
            "alias": "Price",
            "headers": {
                "date": {
                    "type": "set",
                    "alias": "date"
                },
                "symbol": {
                    "type": "set",
                    "alias": "stock symbol"
                },
                "value": {
                    "type": "parameter",
                    "alias": "Price"
                }
            }
        },
        "scalars": {
            "alias": "Input Scalars",
            "symnames": [
                "maxstock",
                "trainingdays"
            ],
            "symtext": [
                "maximum number of stocks to select",
                "number of days for training"
            ],
            "symtypes": [
                "parameter",
                "parameter"
            ],
            "headers": {
                "scalar": {
                    "type": "set",
                    "alias": "Scalar Name"
                },
                "description": {
                    "type": "set",
                    "alias": "Scalar Description"
                },
                "value": {
                    "type": "set",
                    "alias": "Scalar Value"
                }
            }
        }
    },
    "gamsOutputFiles": {
        "partofportfolio": {
            "alias": "what part of the portfolio",
            "headers": {
                "symbol": {
                    "type": "set",
                    "alias": "stock symbol"
                },
                "value": {
                    "type": "parameter",
                    "alias": "what part of the portfolio"
                }
            }
        },
        "dowvsindex": {
            "alias": "dow jones vs. index fund",
            "headers": {
                "date": {
                    "type": "set",
                    "alias": "date"
                },
                "dj": {
                    "type": "parameter",
                    "alias": "dow jones"
                },
                "index fund": {
                    "type": "parameter",
                    "alias": "index fund"
                }
            }
        },
        "abserror": {
            "alias": "absolute error",
            "headers": {
                "date": {
                    "type": "set",
                    "alias": "date"
                },
                "absolute error train": {
                    "type": "parameter",
                    "alias": "absolute error train"
                },
                "absolute error test": {
                    "type": "parameter",
                    "alias": "absolute error test"
                }
            }
        },
        "scalars_out": {
            "alias": "Output Scalars",
            "symnames": [
                "lastDayTraining"
            ],
            "symtext": [
                "last date of training period [MIRO:hidden]"
            ],
            "symtypes": [
                "set"
            ],
            "count": 1,
            "headers": {
                "scalar": {
                    "type": "set",
                    "alias": "Scalar Name"
                },
                "description": {
                    "type": "set",
                    "alias": "Scalar Description"
                },
                "value": {
                    "type": "set",
                    "alias": "Scalar Value"
                }
            }
        }
    }
}