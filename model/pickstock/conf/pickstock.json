{
  "UILogo": "pickstock.png",
  "miroLogFile": "miro.log",
  "remoteImport": [
    {
      "name": "123",
      "templates": [
        {
          "symNames": "price",
          "url": "http://127.0.0.1:5000/io",
          "method": "GET",
          "httpBody": [
            {
              "key": "filename",
              "value": "/Users/fproske/Documents/products/miro/model/pickstock/dowjones2016.csv"
            }
          ]
        }
      ]
    }
  ],
  "remoteExport": [
    {
      "name": "Testexporter",
      "templates": [
        {
          "symNames": "stock_weight",
          "url": "http://127.0.0.1:5000/io",
          "method": "POST",
          "httpBody": [
            {
              "key": "filename",
              "value": "/Users/fproske/Documents/products/miro/model/pickstock/test123.csv"
            }
          ]
        }
      ]
    }
  ],
  "inputWidgets": {
    "price": {
      "widgetType": "table",
      "alias": "Price",
      "readonly": false,
      "heatmap": false,
      "bigData": true
    },
    "maxstock": {
      "widgetType": "slider",
      "label": "Select the maximum number of stocks",
      "alias": "maximum number of stocks to select",
      "min": 1,
      "max": 30,
      "default": 3,
      "step": 1,
      "ticks": false,
      "noHcube": false
    },
    "trainingdays": {
      "label": "Select the number of days for training",
      "min": 1,
      "default": 99,
      "step": 1,
      "max": "card(price$date)",
      "alias": "number of days for training",
      "widgetType": "slider"
    },
    "GMSOPT_mip": {
      "alias": "MIP-Solver",
      "widgetType": "dropdown",
      "label": "Solver to use",
      "choices": ["CBC", "CPLEX", "MOSEK", "SCIP", "XPRESS"],
      "selected": "CPLEX"
    }
  },
  "dataRendering": {
    "price": {
      "outType": "graph",
      "height": 700,
      "graph": {
        "tool": "dygraphs",
        "title": "Stock prices over time (year: 2016)",
        "color": "symbol",
        "dyOptions": {
          "includeZero": true,
          "logscale": false,
          "drawGrid": true,
          "drawPoints": false,
          "pointSize": 2,
          "fillGraph": false,
          "fillAlpha": 0.15
        },
        "dylegend": {
          "width": 450,
          "show": "always",
          "hideOnMouseOut": true
        },
        "dyHighlight": {
          "highlightSeriesOpts": {
            "strokeWidth": 3,
            "strokeBorderWidth": 1,
            "highlightCircleSize": 5
          },
          "hideOnMouseOut": true
        },
        "dyRangeSelector": {
          "height": 20,
          "strokeColor": ""
        },
        "xdata": "date",
        "ydata": {
          "Price": {
            "label": "symbol"
          }
        },
        "xaxis": {
          "title": "date",
          "showgrid": true
        },
        "yaxis": {
          "title": "value",
          "showgrid": true
        }
      }
    },
    "stock_weight": {
      "outType": "dtGraph",
      "graph": {
        "tool": "plotly",
        "type": "pie",
        "showlegend": true,
        "title": "Part of the portfolio",
        "labels": "symbol",
        "values": "value",
        "xaxis": {
          "showgrid": false,
          "zeroline": false,
          "showticklabels": false
        },
        "yaxis": {
          "showgrid": false,
          "zeroline": false,
          "showticklabels": false
        }
      }
    },
    "dowvsindex": {
      "outType": "graph",
      "height": 700,
      "graph": {
        "tool": "dygraphs",
        "title": "Dow Jones vs. Index Fund",
        "dyOptions": {
          "includeZero": false,
          "logscale": false,
          "drawGrid": true,
          "drawPoints": false,
          "pointSize": 2,
          "fillGraph": false,
          "fillAlpha": 0.15
        },
        "dyHighlight": {
          "highlightSeriesOpts": {
            "strokeWidth": 3,
            "strokeBorderWidth": 1,
            "highlightCircleSize": 5
          },
          "hideOnMouseOut": true
        },
        "dylegend": {
          "width": 450,
          "show": "always",
          "hideOnMouseOut": true
        },
        "dyRangeSelector": {
          "height": 20,
          "strokeColor": ""
        },
        "dyEvent": {
          "lastdaytraining": {
            "label": "End of training phase",
            "labelLoc": "bottom",
            "color": "red"
          }
        },
        "xdata": "date",
        "ydata": {
          "dj": {
            "label": "Dow Jones index",
            "stepPlot": false,
            "stemPlot": false,
            "fillGraph": false,
            "drawpoints": true
          },
          "index fund": {
            "label": "Index fund",
            "stepPlot": false,
            "stemPlot": false,
            "fillGraph": false,
            "drawpoints": true
          }
        },
        "xaxis": {
          "title": "Date"
        },
        "yaxis": {
          "title": "Price"
        }
      }
    },
    "abserror": {
      "outType": "dtGraph",
      "height": 700,
      "graph": {
        "tool": "plotly",
        "type": "hist",
        "histnorm": "percent",
        "nbins": 20,
        "barmode": "overlay",
        "alpha": 0.6,
        "title": "Absolute Error: Dow Jones vs. Index Fund",
        "xdata": {
          "absolute error train": {
            "labels": "training",
            "color": "darkgreen",
            "alpha": 0.6
          },
          "absolute error test": {
            "labels": "test",
            "color": "red",
            "alpha": 0.6
          }
        },
        "xaxis": {
          "title": "Absolute Error"
        },
        "yaxis": {
          "title": "Frequency"
        }
      }
    },
    "priceMerge": {
      "outType": "graph",
      "height": 700,
      "graph": {
        "tool": "dygraphs",
        "title": "Price (stocks & dow jones)",
        "color": "uni",
        "dyOptions": {
          "includeZero": true,
          "logscale": false,
          "drawGrid": true,
          "drawPoints": false,
          "pointSize": 2,
          "fillGraph": false,
          "fillAlpha": 0.15
        },
        "dylegend": {
          "width": 450,
          "show": "always",
          "hideOnMouseOut": true
        },
        "dyHighlight": {
          "highlightSeriesOpts": {
            "strokeWidth": 3,
            "strokeBorderWidth": 1,
            "highlightCircleSize": 5
          },
          "hideOnMouseOut": true
        },
        "dyRangeSelector": {
          "height": 20,
          "strokeColor": ""
        },
        "xdata": "date",
        "ydata": {
          "Price (stocks & dow jones)": {
            "label": "symbol"
          }
        },
        "xaxis": {
          "title": "date",
          "showgrid": true
        },
        "yaxis": {
          "title": "value",
          "showgrid": true
        },
        "dyEvent": {
          "lastdaytraining": {
            "label": "End of training phase",
            "labelLoc": "top",
            "color": "red"
          }
        }
      }
    }
  },
  "pivottable": {
    "bgColor": "rgb(255,128,0)"
  },
  "defCompMode": "tab",
  "storeLogFilesDuration": 7,
  "roundingDecimals": 2,
  "activateModules": {
    "scenario": true,
    "strictmode": true,
    "loadLocal": true,
    "logFile": true,
    "lstFile": true,
    "attachments": true
  },
  "aggregateWidgets": true,
  "includeParentDir": false,
  "excelIncludeMeta": true,
  "excelIncludeEmptySheets": true,
  "saveTraceFile": true,
  "autoGenInputGraphs": true,
  "datatable": {
    "class": "display",
    "filter": "none",
    "rownames": false,
    "options": {
      "pageLength": 15
    }
  },
  "theme": "browser",
  "outputGroups": [
    {
      "name": "Dow Jones vs. Index Fund ðŸ™€",
      "members": ["dowvsindex", "abserror"]
    }
  ],
  "hiddenOutputScalars": ["firstdaytraining", "lastdaytraining"],
  "handsontable": {
    "stretchH": "none",
    "readonly": false,
    "highlightCol": true,
    "highlightRow": true,
    "columnSorting": true,
    "manualColumnResize": true,
    "contextMenu": {
      "enabled": true,
      "allowRowEdit": true,
      "allowColEdit": true,
      "allowReadOnly": false,
      "allowComments": true
    },
    "height": 700
  }
}
