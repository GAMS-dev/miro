{
    "pageTitle": "Stock Selection Optimization",
    "MIROSwitch": "MIRO=RUN",
    "fileExchange": "csv",
    "csvDelim": ",",
    "gamsInputFiles": {
        "price": {
            "alias": "Price",
            "headers": {
                "date": {
                    "type": "set",
                    "alias": "date"
                },
                "symbol": {
                    "type": "set",
                    "alias": "stock symbol"
                },
                "value": {
                    "type": "parameter",
                    "alias": "Price"
                }
            }
        },
        "scalars": {
            "alias": "Input Scalars",
            "symnames": [
                "maxstock",
                "trainingdays"
            ],
            "symtext": [
                "maximum number of stocks to select",
                "number of days for training"
            ],
            "symtypes": [
                "parameter",
                "parameter"
            ],
            "headers": {
                "scalar": {
                    "type": "set",
                    "alias": "Scalar Name"
                },
                "description": {
                    "type": "set",
                    "alias": "Scalar Description"
                },
                "value": {
                    "type": "set",
                    "alias": "Scalar Value"
                }
            }
        }
    },
    "gamsOutputFiles": {
        "stock_weight": {
            "alias": "weight",
            "headers": {
                "symbol": {
                    "type": "set",
                    "alias": "stock symbol"
                },
                "value": {
                    "type": "parameter",
                    "alias": "weight"
                }
            }
        },
        "dowvsindex": {
            "alias": "dow jones vs. index fund",
            "headers": {
                "date": {
                    "type": "set",
                    "alias": "date"
                },
                "dj": {
                    "type": "parameter",
                    "alias": "dow jones"
                },
                "index fund": {
                    "type": "parameter",
                    "alias": "index fund"
                }
            }
        },
        "abserror": {
            "alias": "absolute error",
            "headers": {
                "date": {
                    "type": "set",
                    "alias": "date"
                },
                "absolute error train": {
                    "type": "parameter",
                    "alias": "absolute error train"
                },
                "absolute error test": {
                    "type": "parameter",
                    "alias": "absolute error test"
                }
            }
        },
        "pricemerge": {
            "alias": "Price (stocks & dow jones)",
            "headers": {
                "date": {
                    "type": "set",
                    "alias": "date"
                },
                "uni": {
                    "type": "set",
                    "alias": "Universal set"
                },
                "value": {
                    "type": "parameter",
                    "alias": "Price (stocks & dow jones)"
                }
            }
        },
        "scalars_out": {
            "alias": "Output Scalars",
            "symnames": [
                "error_train",
                "error_test",
                "error_ratio",
                "lastdaytraining"
            ],
            "symtext": [
                "Absoulte error in entire training phase",
                "Absoulte error in entire testing phase",
                "Ratio between error test and error train",
                "last date of training period"
            ],
            "symtypes": [
                "parameter",
                "parameter",
                "parameter",
                "set"
            ],
            "count": 4,
            "headers": {
                "scalar": {
                    "type": "set",
                    "alias": "Scalar Name"
                },
                "description": {
                    "type": "set",
                    "alias": "Scalar Description"
                },
                "value": {
                    "type": "set",
                    "alias": "Scalar Value"
                }
            }
        }
    }
