{
    "pageTitle": "Stock Selection Optimization with live data from the web",
    "gamsMetaDelim": "###",
    "gamsWEBUISwitch": "--GMSWEBUI=1",
    "fileExchange": "csv",
    "csvDelim": ",",
    "gamsInputFiles": {
        "maxstock": {
            "alias": "maximum number of stocks to select",
            "slider": {
                "label": "maximum number of stocks to select",
                "min": 1,
                "max": 29,
                "default": 5,
                "step": 1
            }
        },
        "trainingdays": {
            "alias": "number of days for training",
            "slider": {
                "label": "number of days for training",
                "min": 1,
                "max": "card(tw)",
                "default": 99,
                "step": 1
            }
        },
        "GMSPAR_TW": {
            "alias": "time window",
            "daterange": {
                "label": "time window",
                "start": "2016-04-01",
                "startview": "year"
            }
        }
    },
    "gamsOutputFiles": {
        "partofportfolio": {
            "alias": "what part of the portfolio",
            "headers": {
                "stockSymbol": {
                    "type": "set"
                },
                "weight": {
                    "type": "parameter"
                }
            }
        },
        "dowvsindex": {
            "alias": "dow jones vs. index fund",
            "headers": {
                "date": {
                    "type": "set"
                },
                "dow jones": {
                    "type": "parameter"
                },
                "index fund": {
                    "type": "parameter"
                }
            }
        },
        "abserror": {
            "alias": "absolute error",
            "headers": {
                "date": {
                    "type": "set"
                },
                "absolute error train": {
                    "type": "parameter"
                },
                "absolute error test": {
                    "type": "parameter"
                }
            }
        },
        "stockdatarep": {
            "alias": "stock data",
            "headers": {
                "date": {
                    "type": "set"
                },
                "stockSymbol": {
                    "type": "set"
                },
                "price": {
                    "type": "parameter"
                }
            }
        },
        "scalars_out": {
            "alias": "Scalars",
            "hidden": true,
            "count": 5,
            "headers": {
                "Scalar": {
                    "type": "set"
                },
                "Description": {
                    "type": "acronym"
                },
                "Value": {
                    "type": "acronym"
                }
            }
        }
    }
}