{
    "pageTitle": "Stock Selection Optimization with live data from the web",
    "gamsMetaDelim": "###",
    "gamsWEBUISwitch": "--WEBUI=1",
    "fileExchange": "csv",
    "csvDelim": ",",
    "gamsInputFiles": {
        "maxstock": {
            "alias": "maximum number of stocks to select",
            "slider": {
                "label": "maximum number of stocks to select",
                "min": 1,
                "max": 29,
                "default": 5,
                "step": 1
            }
        },
        "trainingdays": {
            "alias": "number of days for training",
            "slider": {
                "label": "number of days for training",
                "min": 1,
                "max": "card(tw)",
                "default": 99,
                "step": 1
            }
        },
        "GMSPAR_TW": {
            "alias": "time window",
            "daterange": {
                "label": "time window",
                "start": "2018-01-01",
                "startview": "year"
            }
        }
    },
    "gamsOutputFiles": {
        "price": {
            "alias": "stock price",
            "headers": {
                "date": {
                    "type": "set"
                },
                "stockSymbol": {
                    "type": "set"
                },
                "stock price": {
                    "type": "parameter"
                }
            }
        },
        "partofportfolio": {
            "alias": "what part of the portfolio",
            "headers": {
                "what part of the portfolio": {
                    "type": "set"
                },
                "partOfPortfolio": {
                    "type": "parameter"
                }
            }
        },
        "dowvsindex": {
            "alias": "dow jones vs. index fund",
            "headers": {
                "date": {
                    "type": "set"
                },
                "dow jones": {
                    "type": "parameter"
                },
                "index fund": {
                    "type": "parameter"
                }
            }
        },
        "abserror": {
            "alias": "absolute error",
            "headers": {
                "date": {
                    "type": "set"
                },
                "absolute error train": {
                    "type": "parameter"
                },
                "absolute error test": {
                    "type": "parameter"
                }
            }
        },
        "scalars_out": {
            "alias": "Output Scalars",
            "hidden": true,
            "symnames": [
                "fHdr",
                "errHdr",
                "lastDayTraining"
            ],
            "symtext": [
                "fund header",
                "stock symbol header",
                "last date of training period"
            ],
            "symtypes": [
                "set",
                "set",
                "set"
            ],
            "count": 3,
            "headers": {
                "Scalar": {
                    "type": "set"
                },
                "Description": {
                    "type": "acronym"
                },
                "Value": {
                    "type": "acronym"
                }
            }
        }
    }
}