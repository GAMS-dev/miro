                "pp": {
                    "type": "set",
                    "alias": "Product"
                },
                "s": {
                    "type": "set",
                    "alias": "Scenario"
                },
                "demand": {
                    "type": "parameter",
                    "alias": "Demand"
                },
                "production time": {
                    "type": "parameter",
                    "alias": "Production Time"
                }
            }
        },
        "scalars": {
            "alias": "Input Scalars",
