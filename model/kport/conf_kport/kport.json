{
  "UILogo": "kport.jpg",
  "inputWidgets": {
    "actScen": {
      "widgetType": "dropdown",
      "alias": "Active scenario",
      "label": "Select scenario",
      "choices": ["$s$", "_"]
    },
    "WHRS": {
      "label": "Select period length [h]",
      "min": 1,
      "max": 672,
      "default": 168,
      "step": 1,
      "alias": "Length of considered period [hours]",
      "widgetType": "slider"
    },
    "CSTI": {
      "label": "Select depreciation cost [k€]",
      "min": 0.01,
      "max": 2.5,
      "default": 0.97,
      "step": 0.01,
      "alias": "Depreciation cost per m^3 reactor and period [kEuro]",
      "widgetType": "slider"
    },
    "CSTF": {
      "label": "Select fixed cost [k€]",
      "min": 1.5,
      "max": 5,
      "default": 2.45,
      "step": 0.05,
      "alias": "Fixed cost per week and reactor [kEuro]",
      "widgetType": "slider"
    },
    "ESF": {
      "label": "Select EOS",
      "min": 0,
      "max": 1,
      "default": 0.5,
      "step": 0.05,
      "alias": "Economies of scale factor",
      "widgetType": "slider"
    }
  },
  "dataRendering": {
    "cost": {
      "outType": "dtGraph",
      "graph": {
        "tool": "plotly",
        "type": "pie",
        "traces": {
          "1": {
            "labels": "chdr",
            "values": "value",
            "hole": 0,
            "name": "Cost report"
          }
        },
        "title": "Overall costs",
        "showlegend": true,
        "staticPlot": false
      },
      "height": 700
    },
    "rpreport": {
      "outType": "miroPivot",
      "height": 700,
      "options": {
        "aggregationFunction": "sum",
        "pivotRenderer": "bar",
        "rows": ["pp", "rr"],
        "cols": {
          "Hdr": null
        }
      }
    },
    "_scalarsve_out": {
      "outType": "miroPivot",
      "height": 700,
      "options": {
        "aggregationFunction": "count",
        "pivotRenderer": "table",
        "rows": ["scalar", "description"],
        "cols": {
          "Hdr": "level"
        }
      }
    },
    "sppx": {
      "outType": "miroPivot",
      "height": 700,
      "options": {
        "aggregationFunction": "count",
        "pivotRenderer": "table",
        "rows": "pp",
        "cols": {
          "Hdr": "level"
        }
      }
    },
    "surplus": {
      "outType": "miroPivot",
      "height": 700,
      "options": {
        "aggregationFunction": "sum",
        "pivotRenderer": "stackedbar",
        "rows": "pp",
        "cols": {
          "Hdr": null
        }
      }
    },
    "productiontime": {
      "outType": "miroPivot",
      "height": 700,
      "options": {
        "aggregationFunction": "sum",
        "pivotRenderer": "table",
        "rows": "rr",
        "cols": {
          "Hdr": null
        }
      }
    }
  },
  "theme": "browser",
  "excelIncludeMeta": true,
  "excelIncludeEmptySheets": true,
  "autoGenInputGraphs": true,
  "storeLogFilesDuration": 7,
  "defCompMode": "split",
  "activateModules": {
    "loadLocal": true,
    "logFile": true,
    "lstFile": true,
    "attachments": true,
    "downloadTempFiles": false
  },
  "aggregateWidgets": true,
  "overwriteSheetOrder": {
    "input": ["_widgets", "rdata", "pdata"],
    "output": ["surplus", "cost", "rpreport", "_scalarsve_out", "sppx", "productiontime"]
  },
  "overwriteAliases": {
    "rdata": {
      "newAlias": "Reactor data"
    },
    "pdata": {
      "newAlias": "Product data"
    },
    "_scalarsve_out": {
      "newAlias": "Output Variable/Equation Scalars"
    },
    "sppx": {
      "newAlias": "compute surplus production p"
    },
    "cost": {
      "newAlias": "Cost report"
    },
    "surplus": {
      "newAlias": "Demand/Surplus report"
    },
    "productiontime": {
      "newAlias": "Production time report"
    },
    "rpreport": {
      "newAlias": "Reactor/Product report"
    }
  },
  "saveTraceFile": true,
  "roundingDecimals": 2,
  "pivottable": {
    "bgColor": "transparent"
  },
  "handsontable": {
    "stretchH": "none",
    "readonly": false,
    "highlightCol": true,
    "highlightRow": true,
    "columnSorting": true,
    "manualColumnResize": true,
    "contextMenu": {
      "enabled": true,
      "allowRowEdit": true,
      "allowColEdit": true,
      "allowReadOnly": false
    },
    "height": 700,
    "colWidths": 150
  }
}
