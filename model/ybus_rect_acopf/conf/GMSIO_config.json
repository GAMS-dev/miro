{
    "pageTitle": "Cost objective calculations for OPF models",
    "gamsMetaDelim": "###",
    "gamsWEBUISwitch": "--GMSWEBUI=1",
    "fileExchange": "csv",
    "csvDelim": ",",
    "gamsInputFiles": {
        "GMSPAR_casename": {
            "alias": "Input case",
            "dropdown": {
                "label": "Select input case",
                "choices": "casename$filename"
            }
        },
        "GMSPAR_timeperiod": {
            "alias": "Timeperiod",
            "slider": {
                "label": "Select the time period to solve",
                "min": 1,
                "max": "max(casename$noPeriods)",
                "default": 1,
                "step": 1
            }
        },
        "GMSPAR_allon": {
            "alias": "allon",
            "dropdown": {
                "label": "Option to turn on all gens or lines during solve",
                "choices": [
                    "gens",
                    "lines",
                    "both",
                    0
                ],
                "selected": "0"
            }
        },
        "GMSPAR_obj": {
            "alias": "Obj",
            "dropdown": {
                "label": "Objective function, piecewise linear or quadratic",
                "aliases": [
                    "quadratic",
                    "piecewise linear",
                    "linear",
                    "none"
                ],
                "choices": [
                    "quad",
                    "pwl",
                    "linear",
                    0
                ],
                "selected": "quad"
            }
        },
        "GMSPAR_linelimits": {
            "alias": "linelimits",
            "dropdown": {
                "label": "Type of line limit data to use",
                "choices": [
                    "given",
                    "uwcalc",
                    "inf"
                ],
                "selected": "given"
            }
        },
        "GMSPAR_genPmin": {
            "alias": "genPmin",
            "dropdown": {
                "label": "Data for Generator lower limit",
                "choices": [
                    "0",
                    "given",
                    "uwcalc"
                ],
                "selected": "given"
            }
        },
        "GMSPAR_qlim": {
            "alias": "qlim",
            "checkbox": {
                "label": "Enforce reactive power limits as D-curve circle constraints?",
                "value": 0,
                "class": "checkbox-material"
            }
        },
        "GMSPAR_savesol": {
            "alias": "savesol",
            "checkbox": {
                "label": "Save solution?",
                "value": 0,
                "class": "checkbox-material"
            }
        },
        "GMSPAR_verbose": {
            "alias": "verbose",
            "checkbox": {
                "label": "Listing printout?",
                "value": 0,
                "class": "checkbox-material"
            }
        },
        "GMSPAR_ic": {
            "alias": "ic",
            "dropdown": {
                "label": "Choose a method for generating initial conditions, i.e. NLP starting point",
                "aliases": [
                    "_",
                    "1: Random",
                    "2: Flat",
                    "3: Random/AC",
                    "4: DC/AC",
                    "5: DC-/AC",
                    "6: Decoupled",
                    "7: DCLoss",
                    "8: Matpower",
                    "9: inputFile"
                ],
                "choices": [
                    "_",
                    1,
                    2,
                    3,
                    4,
                    5,
                    6,
                    7,
                    8,
                    9
                ]
            }
        }
    },
    "gamsOutputFiles": {
        "businfo_report": {
            "alias": "businfo",
            "headers": {
                "Set of buses": {
                    "type": "set"
                },
                "Set of bus info data types": {
                    "type": "set"
                },
                "Set of bus info selectors": {
                    "type": "set"
                },
                "value": {
                    "type": "parameter"
                }
            }
        },
        "geninfo_report": {
            "alias": "geninfo",
            "headers": {
                "Set of generators": {
                    "type": "set"
                },
                "Set of generator info data types": {
                    "type": "set"
                },
                "Set of generator info selectors": {
                    "type": "set"
                },
                "value": {
                    "type": "parameter"
                }
            }
        },
        "branchinfo_report": {
            "alias": "branchinfo",
            "headers": {
                "Set of buses": {
                    "type": "set"
                },
                "Indices for multiple lines between buses": {
                    "type": "set"
                },
                "Set of branch info data types": {
                    "type": "set"
                },
                "Set of branch info selectors": {
                    "type": "set"
                },
                "value": {
                    "type": "parameter"
                }
            }
        },
        "fuelinfo_report": {
            "alias": "fuelinfo",
            "headers": {
                "Set of fuel types": {
                    "type": "set"
                },
                "Set of fuel info selectors": {
                    "type": "set"
                },
                "text": {
                    "type": "parameter"
                }
            }
        },
        "scalars_out": {
            "alias": "Scalars",
            "headers": {
                "Scalar": {
                    "type": "set"
                },
                "Description": {
                    "type": "acronym"
                },
                "Value": {
                    "type": "acronym"
                }
            }
        }
    }
}