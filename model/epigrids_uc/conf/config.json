{
    "inputWidgets"        : {
      "GMSPAR_modelname": {
          "alias": "Modelname",
          "widgetType":"dropdown",
          "label": "Select the model to solve",
          "choices": ["uc_iv", "uc_polar", "uc_rect"],
          "selected": "uc_iv"
      },
      "GMSPAR_casename": {
          "alias": "Input case",
          "widgetType":"dropdown",
          "label": "Select input case [case]",
          "choices": "casename$filename",
          "noHcube": true
      },
      "GMSPAR_times": {
          "alias": "Time range",
          "widgetType":"slider",
          "label": "Select the time range to solve. Note: times > 1 only usable for RTS-96 cases with 24 hour demand data! [times]",
          "min": 1,
          "max": "max(casename$symName[noPeriods]$symVal)",
          "default": [
              1,
              "max(casename$symName[noPeriods]$symVal)"
          ],
          "step": 1
      },
      "GMSPAR_genrun": {
          "alias": "Initial generator conditions",
          "widgetType":"dropdown",
          "label": "Select the initial conditions of the generators",
          "choices": ["_", "$casename$symName[genRun]$symVal"],
          "aliases": ["_", "$casename$symName[genRun]$symDesc"],
          "selected": "_"
      },
      "GMSPAR_allon": {
          "alias": "allon",
          "widgetType":"dropdown",
          "label": "Turn on all gens and/or lines during solve [allon]",
          "aliases": [
              "lines",
              "none"
          ],
          "choices": [
              "lines",
              0
          ],
          "selected": "lines"
      },
      "GMSPAR_obj": {
          "alias": "Obj",
          "widgetType":"dropdown",
          "label": "Objective function [obj]",
          "aliases": [
              "quadratic",
              "piecewise linear"
          ],
          "choices": [
              "quad",
              "pwl"
          ],
          "selected": "pwl"
      },
      "GMSPAR_linelimits": {
          "alias": "linelimits",
          "widgetType":"dropdown",
          "label": "Type of line limit data to use [linelimits]",
          "choices": [
              "given",
              "uwcalc",
              "inf"
          ],
          "selected": "given"
      },
      "GMSPAR_genPmin": {
          "alias": "genPmin",
          "widgetType":"dropdown",
          "label": "Data for Generator lower limit [genPmin]",
          "choices": [
              "0",
              "given",
              "uwcalc"
          ],
          "selected": "given"
      },
      "GMSPAR_ramprates": {
          "alias": "ramprates",
          "widgetType":"dropdown",
          "label": "Type of ramprate data to use [ramprates]",
          "choices": [
              "given",
              "uwcalc"
          ],
          "selected": "given"
      },
      "GMSPAR_relax": {
          "alias": "relax",
          "widgetType":"checkbox",
          "label": "Relax integer models? [relax]",
          "value": 0,
          "class": "checkbox-material"
      },
      "GMSPAR_lineloss": {
          "alias": "lineloss",
          "widgetType":"checkbox",
          "label": "Approximate lineloss? [lineloss]",
          "value": 0,
          "class": "checkbox-material"
      },
      "GMSPAR_wind": {
          "alias": "wind",
          "widgetType":"checkbox",
          "label": "Turn off wind turbines? [wind]",
          "value": 0,
          "max": "max(casename$symName[wind]$symVal)",
          "class": "checkbox-material"
      },
      "GMSPAR_savesol": {
          "alias": "savesol",
          "widgetType":"checkbox",
          "label": "Save solution? [savesol]",
          "value": 0,
          "class": "checkbox-material"
      },
      "GMSPAR_verbose": {
          "alias": "verbose",
          "widgetType":"checkbox",
          "label": "Print input in listing output? [verbose]",
          "value": 1,
          "class": "checkbox-material"
      },
      "GMSPAR_demandbids": {
         "alias": "demandbids",
         "widgetType":"checkbox",
         "label": "Turn on elastic demand bidding? [demandbids]",
         "value": 0,
         "class": "checkbox-material"
      },
      "GMSPAR_qlim": {
         "alias": "qlim",
         "widgetType":"checkbox",
         "label": "Enforce reactive power limits as D-curve circle constraints? [qlim]",
         "value": 0,
         "class": "checkbox-material"
      },
      "GMSPAR_slim": {
         "alias": "slim",
         "widgetType":"checkbox",
         "label": "Use apparent power limits on line?",
         "value": 0,
         "class": "checkbox-material"
      },
      "GMSOPT_rminlp": {
         "alias": "rminlp-Solver",
         "widgetType":"dropdown",
         "label": "Solver to use for rminlp",
         "aliases": ["ANTIGONE","BARON","CONOPT 3","CONOPT 4","COUENNE","IPOPT","KNITRO","LGO","LINDO","LINDOGLOBAL","MINOS","MOSEK","MSNLP","OQNLP","SCIP","SNOPT"],
         "choices": ["ANTIGONE","BARON","CONOPT3","CONOPT4","COUENNE","IPOPT","KNITRO","LGO","LINDO","LINDOGLOBAL","MINOS","MOSEK","MSNLP","OQNLP","SCIP","SNOPT"],
         "selected": "CONOPT3"
      },
      "GMSOPT_minlp": {
         "alias": "minlp-Solver",
         "widgetType":"dropdown",
         "label": "Solver to use for minlp",
         "aliases": ["ALPHAECP","ANTIGONE","BARON","BONMIN","COUENNE","DICOPT","KNITRO","LINDO","LINDOGLOBAL","OQNLP","SBB","SCIP"],
         "choices": ["ALPHAECP","ANTIGONE","BARON","BONMIN","COUENNE","DICOPT","KNITRO","LINDO","LINDOGLOBAL","OQNLP","SBB","SCIP"],
         "selected": "DICOPT"
      }
    },
	"aggregateWidgets"    : {
		"title"				: "Input Options"
    },
    "saveTraceFile":true
}
