{
    "pageTitle": "Cost objective calculations for UC models",
    "gamsMetaDelim": "###",
    "gamsWEBUISwitch": "--GMSWEBUI=1",
    "fileExchange": "csv",
    "csvDelim": ",",
    "gamsInputFiles": {
        "GMSPAR_casename": {
            "alias": "Input case",
            "dropdown": {
                "label": "Select input case",
                "choices": "casename$filename"
            }
        },
        "GMSPAR_times": {
            "alias": "Time range",
            "slider": {
                "label": "Select the time range to solve. Note: times > 1 only usable for RTS-96 cases with 24 hour demand data!",
                "min": 1,
                "max": "max(casename$noPeriods)",
                "default": [
                    1,
                    "max(casename$noPeriods)"
                ],
                "step": 1
            }
        },
        "GMSPAR_allon": {
            "alias": "allon",
            "dropdown": {
                "label": "Turn on all gens and/or lines during solve",
                "aliases": [
                    "lines",
                    "none"
                ],
                "choices": [
                    "lines",
                    0
                ],
                "selected": "0"
            }
        },
        "GMSPAR_obj": {
            "alias": "Obj",
            "dropdown": {
                "label": "Objective function",
                "aliases": [
                    "quadratic",
                    "piecewise linear"
                ],
                "choices": [
                    "quad",
                    "pwl"
                ],
                "selected": "pwl"
            }
        },
        "GMSPAR_linelimits": {
            "alias": "linelimits",
            "dropdown": {
                "label": "Type of line limit data to use",
                "choices": [
                    "given",
                    "uwcalc",
                    "inf"
                ],
                "selected": "given"
            }
        },
        "GMSPAR_genPmin": {
            "alias": "genPmin",
            "dropdown": {
                "label": "Data for Generator lower limit",
                "choices": [
                    "0",
                    "given",
                    "uwcalc"
                ],
                "selected": "given"
            }
        },
        "GMSPAR_savesol": {
            "alias": "savesol",
            "checkbox": {
                "label": "Save solution?",
                "value": 0,
                "class": "checkbox-material"
            }
        },
        "GMSPAR_verbose": {
            "alias": "verbose",
            "checkbox": {
                "label": "Print input in listing output?",
                "value": 1,
                "class": "checkbox-material"
            }
        },
        "GMSPAR_demandbids": {
            "alias": "demandbids",
            "checkbox": {
                "label": "Turn on elastic demand bidding?",
                "value": 0,
                "class": "checkbox-material"
            }
        },
        "GMSPAR_ramprates": {
            "alias": "ramprates",
            "dropdown": {
                "label": "Type of ramprate data to use",
                "choices": [
                    "given",
                    "uwcalc"
                ],
                "selected": "given"
            }
        },
        "GMSPAR_slim": {
            "alias": "slim",
            "checkbox": {
                "label": "Enforce apparent power limits instead of simple bound active line power constraints?",
                "value": 0,
                "class": "checkbox-material"
            }
        },
        "GMSPAR_relax": {
            "alias": "relax",
            "checkbox": {
                "label": "Relax integer models?",
                "value": 0,
                "class": "checkbox-material"
            }
        },
        "GMSPAR_wind": {
            "alias": "wind",
            "checkbox": {
                "label": "Turn off wind turbines?",
                "value": 0,
                "class": "checkbox-material"
            }
        },
        "GMSOPT_solver": {
            "alias": "Solver",
            "dropdown": {
                "label": "Default Solver",
                "aliases": [
                    "ALPHAECP",
                    "ANTIGONE 1.1",
                    "BARON",
                    "BDMLP",
                    "BONMIN 1.8",
                    "CBC 2.9",
                    "CONOPT 3",
                    "CONOPT 4",
                    "COUENNE 0.5",
                    "CPLEX 12.8",
                    "DECIS",
                    "DICOPT",
                    "GLOMIQO 2.3",
                    "GUROBI 8.0",
                    "IPOPT 3.12",
                    "KNITRO 10.3",
                    "LGO",
                    "LINDO 11.0",
                    "LINDOGLOBAL 11.0",
                    "LOCALSOLVER 7.0",
                    "MILES",
                    "MINOS",
                    "MOSEK 8",
                    "MSNLP",
                    "NLPEC",
                    "ODHCPLEX 3.3",
                    "OQNLP",
                    "PATH",
                    "SBB",
                    "SCIP 5.0",
                    "SNOPT",
                    "SOPLEX 3.1",
                    "XA",
                    "XPRESS 32.01"
                ],
                "choices": [
                    "ALPHAECP",
                    "ANTIGONE",
                    "BARON",
                    "BDMLP",
                    "BONMIN",
                    "CBC",
                    "CONOPT3",
                    "CONOPT4",
                    "COUENNE",
                    "CPLEX",
                    "DECIS",
                    "DICOPT",
                    "GLOMIQO",
                    "GUROBI",
                    "IPOPT",
                    "KNITRO",
                    "LGO",
                    "LINDO",
                    "LINDOGLOBAL",
                    "LOCALSOLVER70",
                    "MILES",
                    "MINOS",
                    "MOSEK",
                    "MSNLP",
                    "NLPEC",
                    "ODHCPLEX",
                    "OQNLP",
                    "PATH",
                    "SBB",
                    "SCIP",
                    "SNOPT",
                    "SOPLEX",
                    "XA",
                    "XPRESS"
                ],
                "selected": "CONOPT3"
            }
        },
        "GMSOPT_rminlp": {
            "alias": "rminlp-Solver",
            "dropdown": {
                "label": "Solver to use for rminlp (optional)",
                "aliases": [
                    "_",
                    "ANTIGONE 1.1",
                    "BARON",
                    "CONOPT 3",
                    "CONOPT 4",
                    "COUENNE 0.5",
                    "IPOPT 3.12",
                    "KNITRO 10.3",
                    "LGO",
                    "LINDO 11.0",
                    "LINDOGLOBAL 11.0",
                    "LOCALSOLVER 7.0",
                    "MINOS",
                    "MOSEK 8",
                    "MSNLP",
                    "OQNLP",
                    "SCIP 5.0",
                    "SNOPT"
                ],
                "choices": [
                    "_",
                    "ANTIGONE",
                    "BARON",
                    "CONOPT3",
                    "CONOPT4",
                    "COUENNE",
                    "IPOPT",
                    "KNITRO",
                    "LGO",
                    "LINDO",
                    "LINDOGLOBAL",
                    "LOCALSOLVER70",
                    "MINOS",
                    "MOSEK",
                    "MSNLP",
                    "OQNLP",
                    "SCIP",
                    "SNOPT"
                ],
                "selected": "_"
            }
        },
        "GMSOPT_minlp": {
            "alias": "minlp-Solver",
            "dropdown": {
                "label": "Solver to use for minlp (optional)",
                "aliases": [
                    "_",
                    "ALPHAECP",
                    "ANTIGONE 1.1",
                    "BARON",
                    "BONMIN 1.8",
                    "COUENNE 0.5",
                    "DICOPT",
                    "KNITRO 10.3",
                    "LINDO 11.0",
                    "LINDOGLOBAL 11.0",
                    "LOCALSOLVER 7.0",
                    "OQNLP",
                    "SBB",
                    "SCIP 5.0"
                ],
                "choices": [
                    "_",
                    "ALPHAECP",
                    "ANTIGONE",
                    "BARON",
                    "BONMIN",
                    "COUENNE",
                    "DICOPT",
                    "KNITRO",
                    "LINDO",
                    "LINDOGLOBAL",
                    "LOCALSOLVER70",
                    "OQNLP",
                    "SBB",
                    "SCIP"
                ],
                "selected": "_"
            }
        }
    },
    "gamsOutputFiles": {
        "businfo_report": {
            "alias": "businfo",
            "headers": {
                "Set of buses": {
                    "type": "set"
                },
                "Set of bus info data types": {
                    "type": "set"
                },
                "Set of bus info selectors": {
                    "type": "set"
                },
                "value": {
                    "type": "parameter"
                }
            }
        },
        "geninfo_report": {
            "alias": "geninfo",
            "headers": {
                "Set of generators": {
                    "type": "set"
                },
                "Set of generator info data types": {
                    "type": "set"
                },
                "Set of generator info selectors": {
                    "type": "set"
                },
                "value": {
                    "type": "parameter"
                }
            }
        },
        "branchinfo_report": {
            "alias": "branchinfo",
            "headers": {
                "Set of buses": {
                    "type": "set"
                },
                "Indices for multiple lines between buses": {
                    "type": "set"
                },
                "Set of branch info data types": {
                    "type": "set"
                },
                "Set of branch info selectors": {
                    "type": "set"
                },
                "value": {
                    "type": "parameter"
                }
            }
        },
        "fuelinfo_report": {
            "alias": "fuelinfo",
            "headers": {
                "Set of fuel types": {
                    "type": "set"
                },
                "Set of fuel info selectors": {
                    "type": "set"
                },
                "text": {
                    "type": "parameter"
                }
            }
        },
        "scalars_out": {
            "alias": "Scalars",
            "headers": {
                "Scalar": {
                    "type": "set"
                },
                "Description": {
                    "type": "acronym"
                },
                "Value": {
                    "type": "acronym"
                }
            }
        }
    }
}