{
  "miroLogFile": "miro.log",
  "inputWidgets": {
    "f": {
      "widgetType": "slider",
      "alias": "freight in dollars per case per thousand miles",
      "label": "freight in dollars per case per thousand miles",
      "min": 1,
      "max": 500,
      "default": 90,
      "step": 1
    },
    "mins": {
      "widgetType": "slider",
      "alias": "minimum shipment (MIP- and MINLP-only)",
      "label": "minimum shipment (MIP- and MINLP-only)",
      "min": 0,
      "max": 500,
      "default": 100,
      "step": 1
    },
    "beta": {
      "widgetType": "slider",
      "alias": "beta (MINLP-only)",
      "label": "beta (MINLP-only)",
      "min": 0,
      "max": 1,
      "default": 0.95,
      "step": 0.01
    },
    "_gmspar_type": {
      "widgetType": "dropdown",
      "alias": "Model Type",
      "label": "Select model type",
      "aliases": ["LP", "MIP", "MINLP"],
      "choices": ["lp", "mip", "minlp"],
      "selected": "lp"
    },
    "d": {
      "widgetType": "table",
      "alias": "distance in thousands of miles",
      "readonly": false,
      "heatmap": false,
      "bigData": false,
      "pivotCols": "i"
    }
  },
  "dataRendering": {
    "schedule": {
      "outType": "transport1",
      "packages": ["leaflet", "leaflet.minicharts"]
    },
    "_scalars_out": {
      "outType": "valueBox",
      "options": {
        "color": "aqua"
      }
    }
  },
  "pivottable": {
    "bgColor": "rgb(255,128,0)"
  },
  "theme": "browser",
  "defCompMode": "split",
  "roundingDecimals": 2,
  "activateModules": {
    "logFile": true,
    "lstFile": true,
    "loadLocal": true,
    "attachments": true,
    "remoteExecution": false,
    "downloadTempFiles": false
  },
  "autoGenInputGraphs": true,
  "aggregateWidgets": true,
  "storeLogFilesDuration": 7,
  "excelIncludeMeta": true,
  "excelIncludeEmptySheets": true,
  "saveTraceFile": true,
  "includeParentDir": false,
  "datatable": {
    "class": "display",
    "filter": "none",
    "rownames": false,
    "options": {
      "pageLength": 15
    }
  },
  "handsontable": {
    "stretchH": "none",
    "readonly": false,
    "highlightCol": true,
    "highlightRow": true,
    "columnSorting": true,
    "manualColumnResize": true,
    "contextMenu": {
      "enabled": true,
      "allowRowEdit": true,
      "allowColEdit": true,
      "allowReadOnly": false,
      "allowComments": true
    },
    "height": 700,
    "colWidths": 300
  }
}
