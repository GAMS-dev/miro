{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "root": true,
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 13
  },
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ],
    "no-multi-str": "off",
    "no-underscore-dangle": [
      2,
      {
        "allowAfterThis": true
      }
    ]
  }
}
