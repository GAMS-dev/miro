{
  "parser": "@babel/eslint-parser",
  "env": {
    "browser": true
  },
  "rules": {
    "func-names": ["error", "never"]
  },
  "extends": ["airbnb-base"],
  "root": true,
  "parserOptions": {
    "requireConfigFile": false,
    "babelOptions": {
      "plugins": ["@babel/plugin-proposal-class-properties"]
    }
  }
}
