{
    "activateModules": {
        "attachments": true,
        "downloadTempFiles": false,
        "hcube": false,
        "loadLocal": true,
        "logFile": true,
        "lstFile": true
    },
    "aggregateWidgets": true,
    "autoGenInputGraphs": true,
    "customCss": false,
    "dataRendering": {
        "cohortreport": {
            "height": 700,
            "options": {
                "aggregationFunction": "count",
                "cols": {
                    "uni": null
                },
                "emptyUEL": "-",
                "enableHideEmptyCols": false,
                "pivotRenderer": "table",
                "rows": "c"
            },
            "outType": "miroPivot"
        },
        "groupreport": {
            "height": 700,
            "options": {
                "aggregationFunction": "count",
                "cols": {
                    "uni": null
                },
                "emptyUEL": "-",
                "enableHideEmptyCols": false,
                "pivotRenderer": "table",
                "rows": "g"
            },
            "outType": "miroPivot"
        },
        "studentgroupcohort": {
            "height": 700,
            "options": {
                "aggregationFunction": "count",
                "aggregations": {
                    "gc": [
                        "A",
                        "B"
                    ]
                },
                "cols": {
                    "c": null
                },
                "emptyUEL": "-",
                "enableHideEmptyCols": false,
                "pivotRenderer": "table",
                "rows": "s"
            },
            "outType": "miroPivot"
        }
    },
    "datatable": {
        "class": "display",
        "filter": "bottom",
        "options": {
            "pageLength": 15
        },
        "rownames": false
    },
    "defaultRendererOutput": "miroPivot",
    "defaultScenName": "default",
    "defCompMode": "split",
    "excelIncludeEmptySheets": true,
    "excelIncludeMeta": true,
    "handsontable": {
        "colWidths": 200,
        "contextMenu": {
            "allowColEdit": true,
            "allowRowEdit": true,
            "enabled": true
        },
        "height": 700,
        "readonly": false,
        "stretchH": "none"
    },
    "inputGroups": [
        {
            "members": [
                "groupdata",
                "gsmap"
            ],
            "name": "Students / Groups",
            "sameTab": false
        }
    ],
    "inputWidgetGroups": [
        {
            "members": "priorityab",
            "name": "Priority to keep the input assignment",
            "sameTab": true
        }
    ],
    "inputWidgets": {
        "existingcohortgroupmap": {
            "apiVersion": 2,
            "rendererName": "mirorenderer_existingcohortgroupmap",
            "widgetType": "custom"
        },
        "groupdata": {
            "dropdownCols": {
                "mode": {
                    "colType": "dropdown",
                    "static": [
                        "split",
                        "split even",
                        "prevent split"
                    ]
                }
            },
            "heatmap": false,
            "hideIndexCol": false,
            "readonly": false,
            "tableType": "default",
            "widgetType": "table"
        },
        "gsmap": {
            "dropdownCols": {
                "g": {
                    "colType": "dropdown",
                    "column": "g",
                    "symbol": "groupdata"
                }
            },
            "heatmap": false,
            "hideIndexCol": false,
            "readonly": false,
            "tableType": "default",
            "widgetType": "table"
        },
        "priorityab": {
            "alias": "priority to keep the input assignment",
            "default": 1,
            "label": "priority to keep the existing cohort map",
            "max": 99,
            "min": 0,
            "noHcube": false,
            "step": 1,
            "ticks": false,
            "widgetType": "slider"
        }
    },
    "layoutSettings": {
        "maxTabsExpandedInput": 5,
        "maxTabsExpandedOutput": 5,
        "maxTabsExpandedPivotComp": 5,
        "maxTabsExpandedSplitComp": 5,
        "maxTabsExpandedTabComp": 5
    },
    "outputTables": {
        "cohortreport": {
            "class": "display",
            "filter": "bottom",
            "options": {
                "decimals": 0,
                "pageLength": 15
            },
            "pivotCols": "uni",
            "rownames": false
        },
        "groupreport": {
            "class": "display",
            "filter": "bottom",
            "options": {
                "decimals": 0,
                "pageLength": 15
            },
            "pivotCols": "uni",
            "rownames": false
        }
    },
    "overwriteAliases": {
        "_scalars": {
            "newAlias": "Scalars"
        },
        "cohortreport": {
            "newAlias": "Report on cohort"
        },
        "existingcohortgroupmap": {
            "newAlias": "Existing cohort map"
        },
        "groupdata": {
            "newAlias": "Groups"
        },
        "groupreport": {
            "newAlias": "Report on groups"
        },
        "gsmap": {
            "newAlias": "Students to groups mapping"
        },
        "priorityab": {
            "newAlias": "priority to keep the input assignment"
        },
        "studentgroupcohort": {
            "newAlias": "Student assignment"
        }
    },
    "overwriteHeaderAliases": {
        "cohortreport": {
            "newHeaders": [
                "Cohorts",
                "Size / (move) penalty",
                "Value"
            ]
        },
        "existingcohortgroupmap": {
            "newHeaders": [
                "Students",
                "Groups",
                "Cohorts",
                "Comment"
            ]
        },
        "groupdata": {
            "newHeaders": [
                "Groups",
                "Splitting mode",
                "Max group size",
                "Priority"
            ]
        },
        "groupreport": {
            "newHeaders": [
                "Groups",
                "Size / penalty",
                "Value"
            ]
        },
        "gsmap": {
            "newHeaders": [
                "Groups",
                "Students",
                "Comment"
            ]
        },
        "studentgroupcohort": {
            "newHeaders": [
                "students",
                "group/cohort",
                "cohorts",
                "Set text Student assignment"
            ]
        }
    },
    "overwriteSheetOrder": {
        "input": [
            "groupdata",
            "gsmap",
            "existingcohortgroupmap",
            "_widgets",
            "_widgets1"
        ]
    },
    "pivotCompSettings": {
        "fixedColumns": true,
        "hidePivotControls": false
    },
    "readme": {
        "enableMath": false,
        "filename": "README.md",
        "tabTitle": "Readme"
    },
    "roundingDecimals": 0,
    "saveTraceFile": true,
    "storeLogFilesDuration": 7,
    "symbolLinks": [
        {
            "source": "studentgroupcohort",
            "target": "existingcohortgroupmap"
        }
    ],
    "theme": "browser",
    "UILogo": "cohort.png"
}
