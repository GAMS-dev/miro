{
  "overwriteSheetOrder": {
    "input": "_widgets"
  },
  "overwriteAliases": {
    "_scalars": {
      "newAlias": "Scalars"
    },
    "_scalars_out": {
      "newAlias": "Output Scalars"
    },
    "pressurethickness": {
      "newAlias": "pressurethickness"
    }
  },
  "theme": "browser",
  "defCompMode": "split",
  "roundingDecimals": 2,
  "activateModules": {
    "logFile": true,
    "lstFile": true,
    "loadLocal": true,
    "attachments": true,
    "remoteExecution": false,
    "downloadTempFiles": false
  },
  "autoGenInputGraphs": true,
  "aggregateWidgets": true,
  "storeLogFilesDuration": 7,
  "excelIncludeMeta": true,
  "excelIncludeEmptySheets": true,
  "saveTraceFile": true,
  "pageTitle": "Lubrication",
  "UILogo": "lubrication-regimes-elastohydrodynamic.jpg",
  "defaultScenName": "default",
  "datatable": {
    "class": "display",
    "filter": "none",
    "rownames": false,
    "options": {
      "pageLength": 15
    }
  },
  "inputWidgets": {
    "npoints": {
      "widgetType": "slider",
      "alias": "npoints",
      "min": 10,
      "max": 1000,
      "default": 100,
      "step": 10,
      "ticks": true,
      "noHcube": false,
      "label": "Discretization points"
    },
    "load": {
      "widgetType": "slider",
      "alias": "load on cylinder in lbs",
      "min": 80,
      "max": 300,
      "default": 100,
      "step": 5,
      "ticks": true,
      "noHcube": false,
      "label": "Load on cylinder (pounds)"
    },
    "speed": {
      "widgetType": "slider",
      "alias": "cylinder RPM",
      "min": 300,
      "max": 8000,
      "default": 800,
      "step": 50,
      "ticks": true,
      "noHcube": false,
      "label": "cylinder (bearing) RPM"
    }
  },
  "dataRendering": {
    "pressurethickness": {
      "outType": "graph",
      "graph": {
        "title": "Pressure & Thickness",
        "tool": "plotly",
        "type": "scatter",
        "ydata": {
          "pressure": {
            "label": "pressure",
            "mode": "lines",
            "line": {
              "width": 2,
              "shape": "linear",
              "dash": "solid"
            },
            "showlegend": true,
            "yaxis": "y"
          },
          "thickness": {
            "label": "thickness",
            "mode": "lines",
            "line": {
              "width": 2,
              "shape": "linear",
              "dash": "solid"
            },
            "showlegend": true,
            "yaxis": "y2"
          }
        },
        "xdata": "x",
        "showlegend": true,
        "xaxis": {
          "title": "x",
          "showgrid": false,
          "zeroline": false,
          "showticklabels": true,
          "categoryorder": "trace"
        },
        "yaxis": {
          "title": "pressure",
          "showgrid": false,
          "zeroline": false,
          "showticklabels": true,
          "categoryorder": "trace",
          "rangefrom": "",
          "rangeto": ""
        },
        "y2axis": {
          "title": "thickness",
          "showgrid": false,
          "zeroline": false,
          "showticklabels": true,
          "categoryorder": "trace",
          "rangefrom": "",
          "rangeto": ""
        }
      },
      "height": 700
    }
  },
  "readme": {
    "tabTitle": "Introduction",
    "filename": "README.md",
    "enableMath": false
  },
  "handsontable": {
    "stretchH": "none",
    "readonly": false,
    "highlightCol": true,
    "highlightRow": true,
    "columnSorting": true,
    "manualColumnResize": true,
    "contextMenu": {
      "enabled": true,
      "allowRowEdit": true,
      "allowColEdit": true,
      "allowReadOnly": false
    },
    "height": 700,
    "colWidths": 150
  }
}
