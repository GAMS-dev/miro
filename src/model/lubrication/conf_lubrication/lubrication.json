{
    "activateModules": {
        "attachments": true,
        "downloadTempFiles": false,
        "hcube": false,
        "loadLocal": true,
        "logFile": true,
        "lstFile": true,
        "remoteExecution": false
    },
    "aggregateWidgets": true,
    "autoGenInputGraphs": true,
    "customCss": false,
    "dataRendering": {
        "pressurethickness": {
            "graph": {
                "showlegend": true,
                "title": "Pressure & Thickness",
                "tool": "plotly",
                "type": "scatter",
                "xaxis": {
                    "categoryorder": "trace",
                    "showgrid": false,
                    "showticklabels": true,
                    "title": "x",
                    "zeroline": false
                },
                "xdata": "x",
                "y2axis": {
                    "categoryorder": "trace",
                    "rangefrom": "",
                    "rangeto": "",
                    "showgrid": false,
                    "showticklabels": true,
                    "title": "thickness",
                    "zeroline": false
                },
                "yaxis": {
                    "categoryorder": "trace",
                    "rangefrom": "",
                    "rangeto": "",
                    "showgrid": false,
                    "showticklabels": true,
                    "title": "pressure",
                    "zeroline": false
                },
                "ydata": {
                    "pressure": {
                        "label": "pressure",
                        "line": {
                            "dash": "solid",
                            "shape": "linear",
                            "width": 2
                        },
                        "mode": "lines",
                        "showlegend": true,
                        "yaxis": "y"
                    },
                    "thickness": {
                        "label": "thickness",
                        "line": {
                            "dash": "solid",
                            "shape": "linear",
                            "width": 2
                        },
                        "mode": "lines",
                        "showlegend": true,
                        "yaxis": "y2"
                    }
                }
            },
            "height": 700,
            "outType": "graph"
        }
    },
    "datatable": {
        "class": "display",
        "filter": "none",
        "options": {
            "pageLength": 15
        },
        "rownames": false
    },
    "defaultRendererOutput": "miroPivot",
    "defaultScenName": "default",
    "defCompMode": "split",
    "excelIncludeEmptySheets": true,
    "excelIncludeMeta": true,
    "handsontable": {
        "colWidths": 150,
        "contextMenu": {
            "allowColEdit": true,
            "allowRowEdit": true,
            "enabled": true
        },
        "height": 700,
        "readonly": false,
        "stretchH": "none"
    },
    "inputWidgets": {
        "load": {
            "alias": "load on cylinder in lbs",
            "default": 100,
            "label": "Load on cylinder (pounds)",
            "max": 300,
            "min": 80,
            "noHcube": false,
            "step": 5,
            "ticks": true,
            "widgetType": "slider"
        },
        "npoints": {
            "alias": "npoints",
            "default": 100,
            "label": "Discretization points",
            "max": 1000,
            "min": 10,
            "noHcube": false,
            "step": 10,
            "ticks": true,
            "widgetType": "slider"
        },
        "speed": {
            "alias": "cylinder RPM",
            "default": 800,
            "label": "cylinder (bearing) RPM",
            "max": 8000,
            "min": 300,
            "noHcube": false,
            "step": 50,
            "ticks": true,
            "widgetType": "slider"
        }
    },
    "layoutSettings": {
        "maxTabsExpandedInput": 5,
        "maxTabsExpandedOutput": 5,
        "maxTabsExpandedPivotComp": 5,
        "maxTabsExpandedSplitComp": 5,
        "maxTabsExpandedTabComp": 5
    },
    "overwriteAliases": {
        "_scalars": {
            "newAlias": "Scalars"
        },
        "_scalars_out": {
            "newAlias": "Output Scalars"
        },
        "alpha": {
            "newAlias": "pressure viscosity coefficient"
        },
        "lambda": {
            "newAlias": "model parameter used in Reynolds equation"
        },
        "load": {
            "newAlias": "load on cylinder in lbs"
        },
        "npoints": {
            "newAlias": "npoints"
        },
        "pressurethickness": {
            "newAlias": "pressurethickness"
        },
        "speed": {
            "newAlias": "cylinder RPM"
        }
    },
    "overwriteSheetOrder": {
        "input": "_widgets"
    },
    "pageTitle": "Lubrication",
    "pivotCompSettings": {
        "fixedColumns": true,
        "hidePivotControls": false
    },
    "readme": {
        "enableMath": false,
        "filename": "README.md",
        "tabTitle": "Introduction"
    },
    "roundingDecimals": 2,
    "saveTraceFile": true,
    "storeLogFilesDuration": 7,
    "theme": "browser",
    "UILogo": "lubrication-regimes-elastohydrodynamic.jpg"
}
