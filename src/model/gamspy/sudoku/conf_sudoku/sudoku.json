{
    "activateModules": {
        "attachments": true,
        "downloadTempFiles": false,
        "hcube": false,
        "loadLocal": true,
        "logFile": true,
        "lstFile": false
    },
    "aggregateWidgets": true,
    "autoGenInputGraphs": true,
    "customCss": false,
    "dataRendering": {
        "initial_state": {
            "outType": "mirowidget_initial_state"
        },
        "results": {
            "outType": "mirowidget_initial_state"
        }
    },
    "datatable": {
        "class": "display",
        "filter": "bottom",
        "options": {
            "pageLength": 15
        },
        "rownames": false
    },
    "defaultRendererOutput": "miroPivot",
    "defCompMode": "split",
    "excelIncludeEmptySheets": true,
    "excelIncludeMeta": true,
    "handsontable": {
        "colWidths": 150,
        "contextMenu": {
            "allowColEdit": true,
            "allowRowEdit": true,
            "enabled": true
        },
        "height": 700,
        "readonly": false,
        "stretchH": "none"
    },
    "inputWidgets": {
        "force_unique_sol": {
            "alias": "Force unique solution",
            "class": "checkbox-material",
            "label": "Compute only if solution is unique?",
            "noHcube": false,
            "value": 1,
            "widgetType": "checkbox"
        },
        "initial_state": {
            "alias": "Initial state",
            "apiVersion": 2,
            "options": {
                "isInput": true
            },
            "rendererName": "mirowidget_initial_state",
            "widgetSymbols": "force_unique_sol",
            "widgetType": "custom"
        }
    },
    "layoutSettings": {
        "maxTabsExpandedInput": 5,
        "maxTabsExpandedOutput": 5,
        "maxTabsExpandedPivotComp": 5,
        "maxTabsExpandedSplitComp": 5,
        "maxTabsExpandedTabComp": 5
    },
    "overwriteAliases": {
        "force_unique_sol": {
            "newAlias": "force_unique_sol"
        },
        "initial_state": {
            "newAlias": "initial_state"
        },
        "results": {
            "newAlias": "results"
        }
    },
    "overwriteSheetOrder": {
        "input": [
            "initial_state",
            "_widgets"
        ]
    },
    "pageTitle": "Sudoku solver",
    "pivotCompSettings": {
        "fixedColumns": true,
        "hidePivotControls": false
    },
    "roundingDecimals": 2,
    "saveTraceFile": true,
    "storeLogFilesDuration": 7,
    "theme": "browser",
    "parseLogForMiroLogSyntax": true
}
