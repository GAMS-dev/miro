{
    "activateModules": {
        "attachments": true,
        "downloadTempFiles": false,
        "hcube": false,
        "loadLocal": true,
        "logFile": true,
        "lstFile": true
    },
    "aggregateWidgets": true,
    "autoGenInputGraphs": true,
    "customCss": false,
    "dataRendering": {
        "circledata": {
            "graph": {
                "fixedHeight": 700,
                "fixedWidth": 700,
                "showlegend": false,
                "staticPlot": true,
                "title": "Optimized (maximal) radius",
                "tool": "plotly",
                "type": "bubble",
                "xaxis": {
                    "categoryorder": "trace",
                    "rangefrom": -1.1,
                    "rangeto": 1.1,
                    "showgrid": false,
                    "showticklabels": true,
                    "title": "x",
                    "zeroline": false
                },
                "xdata": "x",
                "yaxis": {
                    "categoryorder": "trace",
                    "rangefrom": -1.1,
                    "rangeto": 1.1,
                    "scaleanchor": "x",
                    "scaleratio": 1,
                    "showgrid": false,
                    "showticklabels": true,
                    "title": "y",
                    "zeroline": false
                },
                "ydata": {
                    "y": {
                        "label": "y",
                        "marker": {
                            "color": "#ff8c00",
                            "line": {
                                "width": "3"
                            },
                            "maxsize": 33.85,
                            "opacity": 1,
                            "size": "radius",
                            "sizemode": "diameter",
                            "symbol": "circle-open"
                        },
                        "mode": "markers",
                        "showlegend": false
                    }
                }
            },
            "height": 700,
            "outType": "graph"
        }
    },
    "datatable": {
        "class": "display",
        "filter": "none",
        "options": {
            "pageLength": 15
        },
        "rownames": false
    },
    "defaultRendererOutput": "miroPivot",
    "defCompMode": "split",
    "excelIncludeEmptySheets": true,
    "excelIncludeMeta": true,
    "handsontable": {
        "colWidths": 150,
        "contextMenu": {
            "allowColEdit": true,
            "allowRowEdit": true,
            "enabled": true
        },
        "height": 700,
        "readonly": false,
        "stretchH": "none"
    },
    "inputWidgets": {
        "k": {
            "alias": "number of circles",
            "default": 5,
            "label": "Select the number of inner circles to be placed in the outer circle",
            "max": 100,
            "min": 1,
            "noHcube": false,
            "step": 1,
            "ticks": true,
            "widgetType": "slider"
        },
        "optcrval": {
            "alias": "relative optimality criterion",
            "decimal": 3,
            "label": "Define the relative optimality criterion for global solvers",
            "max": 1,
            "min": 0,
            "sign": null,
            "value": 0.1,
            "widgetType": "numericinput"
        },
        "qcp": {
            "alias": "QCP solver",
            "choices": [
                "baron",
                "conopt",
                "conopt4",
                "gurobi",
                "ipopt",
                "knitro",
                "minos",
                "scip",
                "snopt"
            ],
            "label": "select the QCP solver",
            "multiple": false,
            "noHcube": false,
            "selected": "scip",
            "widgetType": "dropdown"
        },
        "reslimval": {
            "alias": "solver time limit in seconds",
            "default": 10,
            "label": "choose a solver time limit in seconds",
            "max": 120,
            "min": 0,
            "noHcube": false,
            "step": 1,
            "ticks": false,
            "widgetType": "slider"
        }
    },
    "layoutSettings": {
        "maxTabsExpandedInput": 5,
        "maxTabsExpandedOutput": 5,
        "maxTabsExpandedPivotComp": 5,
        "maxTabsExpandedSplitComp": 5,
        "maxTabsExpandedTabComp": 5
    },
    "miroLogFile": "cpack_miroLog.dat",
    "overwriteAliases": {
        "circledata": {
            "newAlias": "circledata"
        },
        "k": {
            "newAlias": "number of circles"
        },
        "optcrval": {
            "newAlias": "optcr"
        },
        "qcp": {
            "newAlias": "QCP solver to use"
        },
        "reslimval": {
            "newAlias": "reslim"
        }
    },
    "overwriteSheetOrder": {
        "input": "_widgets"
    },
    "pageTitle": "Circle Packing",
    "pivotCompSettings": {
        "fixedColumns": true,
        "hidePivotControls": false
    },
    "readme": {
        "enableMath": false,
        "filename": "README.md",
        "tabTitle": "README"
    },
    "roundingDecimals": 4,
    "storeLogFilesDuration": 7,
    "theme": "browser",
    "UILogo": "cpack.png"
}
