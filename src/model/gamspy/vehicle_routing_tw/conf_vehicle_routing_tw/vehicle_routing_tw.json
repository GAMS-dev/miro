{
    "activateModules": {
        "attachments": true,
        "downloadTempFiles": false,
        "hcube": false,
        "loadLocal": true,
        "logFile": true,
        "lstFile": false
    },
    "aggregateWidgets": true,
    "autoGenInputGraphs": true,
    "customCss": false,
    "dataRendering": {
        "x": {
            "additionalData": [
                "customerdata",
                "t",
                "w"
            ],
            "height": 800,
            "options": {
                "title": "Optimal vehicle routes"
            },
            "outType": "mirorenderer_x",
            "packages": "leaflet.minicharts"
        }
    },
    "defaultRendererOutput": "miroPivot",
    "defaultScenName": "default",
    "defCompMode": "split",
    "excelIncludeEmptySheets": true,
    "excelIncludeMeta": true,
    "handsontable": {
        "colWidths": 200,
        "contextMenu": {
            "allowColEdit": true,
            "allowRowEdit": true,
            "enabled": true
        },
        "height": 700,
        "readonly": false,
        "stretchH": "none"
    },
    "hiddenOutputSymbols": [
        "t",
        "w"
    ],
    "inputWidgets": {
        "customerdata": {
            "alias": "Customer informations",
            "apiVersion": 2,
            "rendererName": "mirowidget_customerdata",
            "widgetType": "custom"
        },
        "vehicle_capacity": {
            "decimal": 0,
            "decimalCharacter": ".",
            "digitGroupSeparator": ",",
            "label": "Capacity of the vehicles",
            "max": null,
            "min": 0,
            "sign": null,
            "value": 200,
            "widgetType": "numericinput"
        },
        "vehicle_number": {
            "decimal": 0,
            "decimalCharacter": ".",
            "digitGroupSeparator": ",",
            "label": "Number of available vehicles",
            "min": 1,
            "value": 10,
            "widgetType": "numericinput"
        }
    },
    "layoutSettings": {
        "maxTabsExpandedInput": 5,
        "maxTabsExpandedOutput": 5,
        "maxTabsExpandedPivotComp": 5,
        "maxTabsExpandedSplitComp": 5,
        "maxTabsExpandedTabComp": 5
    },
    "overwriteAliases": {
        "_scalars": {
            "newAlias": "Scalars"
        },
        "customerdata": {
            "newAlias": "Cusotmer information"
        },
        "t": {
            "newAlias": "arrival time at customer ci"
        },
        "vehicle_capacity": {
            "newAlias": "vehicle_capacity"
        },
        "vehicle_number": {
            "newAlias": "vehicle_number"
        },
        "w": {
            "newAlias": "waiting time at customer ci: ei-ti"
        },
        "x": {
            "newAlias": "states if vehicle k travels from ci to cj"
        }
    },
    "overwriteSheetOrder": {
        "input": [
            "customerdata",
            "_widgets"
        ]
    },
    "pageTitle": "Vehicle Routing Problem",
    "pivotCompSettings": {
        "fixedColumns": true,
        "hidePivotControls": false
    },
    "readme": {
        "enableMath": true,
        "filename": "README.md",
        "tabTitle": "Introduction"
    },
    "roundingDecimals": 2,
    "storeLogFilesDuration": 7,
    "theme": "browser"
}
