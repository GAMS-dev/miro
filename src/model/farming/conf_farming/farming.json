{
  "overwriteSheetOrder": {
    "input": ["cd", "pricecurve", "_widgets"],
    "output": ["rep", "repcompare", "repfinance", "repc"]
  },
  "theme": "browser",
  "defCompMode": "split",
  "roundingDecimals": 2,
  "activateModules": {
    "logFile": false,
    "lstFile": false,
    "loadLocal": true,
    "attachments": false,
    "downloadTempFiles": true,
    "hcube": true
  },
  "autoGenInputGraphs": true,
  "aggregateWidgets": true,
  "storeLogFilesDuration": 7,
  "excelIncludeMeta": true,
  "excelIncludeEmptySheets": true,
  "saveTraceFile": true,
  "pageTitle": "Crop planting",
  "miroLogFile": "farming_miro.log",
  "UILogo": "jack-bain-mEgPe7RSBTc-unsplash-cut.jpg",
  "defaultScenName": "default",
  "inputWidgets": {
    "cd": {
      "widgetType": "table",
      "alias": "crop data",
      "readonly": false,
      "heatmap": false,
      "bigData": false
    },
    "pricecurve": {
      "widgetType": "table",
      "alias": "price curve data",
      "readonly": false,
      "heatmap": false,
      "bigData": false
    },
    "yf": {
      "widgetType": "slider",
      "alias": "yield factor",
      "min": 0,
      "max": 2,
      "default": 1,
      "step": 0.5,
      "ticks": true,
      "noHcube": false,
      "label": "Yield factor"
    },
    "land": {
      "widgetType": "slider",
      "alias": "available land [acre]",
      "min": 200,
      "max": 1000,
      "default": 500,
      "step": 25,
      "ticks": true,
      "noHcube": false,
      "label": "Available land [acre]"
    },
    "_gmsopt_emp": {
      "widgetType": "dropdown",
      "alias": "Stochastic solver",
      "choices": ["DE", "DECIS", "LINDO"],
      "selected": "DE",
      "noHcube": false,
      "multiple": false,
      "label": "Stochastic solver"
    },
    "nums": {
      "widgetType": "slider",
      "alias": "number of scenarios",
      "min": 0,
      "max": 50,
      "default": 0,
      "step": 1,
      "ticks": true,
      "noHcube": false,
      "label": "Number of scenarios"
    }
  },
  "dataRendering": {
    "repfinance": {
      "outType": "graph",
      "graph": {
        "title": "financial report by scenario",
        "tool": "plotly",
        "type": "hist",
        "traces": {
          "1": {
            "labels": "s",
            "values": "value",
            "hole": 0,
            "name": "financial report by scenario"
          }
        },
        "showlegend": true,
        "xdata": {
          "value": {}
        },
        "histnorm": "",
        "nbins": 25,
        "barmode": "overlay",
        "alpha": 0.6,
        "xaxis": {
          "title": "financial report by scenario"
        },
        "cumulative": false,
        "horizontal": false,
        "yaxis": {
          "title": ""
        },
        "dragmode": "zoom",
        "color": "rh",
        "filter": {
          "col": "rh",
          "label": "",
          "multiple": false
        }
      },
      "height": 700
    },
    "rep": {
      "outType": "mirorenderer_rep",
      "additionalData": ["cd", "pricecurve", "repc", "repcompare", "repfinance"]
    },
    "repcompare": {
      "outType": "miroPivot",
      "height": 700,
      "options": {
        "aggregationFunction": "sum",
        "pivotRenderer": "bar",
        "enableHideEmptyCols": false,
        "hideEmptyCols": false,
        "hidePivotControls": false,
        "emptyUEL": "-",
        "rows": "c",
        "cols": {
          "Hdr": null
        }
      }
    },
    "repc": {
      "outType": "graph",
      "graph": {
        "tool": "plotly",
        "type": "pie",
        "traces": {
          "1": {
            "labels": "c",
            "values": "yield",
            "hole": 0.4,
            "name": "crop yield [tons]"
          },
          "2": {
            "labels": "c",
            "values": "planted",
            "name": "crop planted [acres]",
            "hole": 0.4
          },
          "3": {
            "labels": "c",
            "values": "seedcost",
            "name": "seed cost [$]",
            "hole": 0.4
          },
          "4": {
            "labels": "c",
            "values": "sold",
            "name": "crop sold [tons]",
            "hole": 0.4
          },
          "5": {
            "labels": "c",
            "values": "sales",
            "name": "sales",
            "hole": 0.4
          },
          "6": {
            "labels": "c",
            "values": "purchased",
            "name": "crop purchased [tons]",
            "hole": 0.4
          },
          "7": {
            "labels": "c",
            "values": "pcost",
            "name": "purchase cost [$]",
            "hole": 0.4
          }
        },
        "title": "crop report",
        "showlegend": true,
        "staticPlot": false
      },
      "height": 700
    }
  },
  "readme": {
    "tabTitle": "Crop plant alloction problem with stochastic crop yield",
    "filename": "README.md",
    "enableMath": false
  },
  "defaultRendererOutput": "miroPivot",
  "overwriteAliases": {
    "yf": {
      "newAlias": "Yield Factor"
    },
    "land": {
      "newAlias": "Available Land [acre]"
    },
    "nums": {
      "newAlias": "Number of Scenarios"
    },
    "cd": {
      "newAlias": "Crop Data"
    },
    "pricecurve": {
      "newAlias": "Price Curve Data"
    },
    "rep": {
      "newAlias": "Dashboard"
    },
    "repc": {
      "newAlias": "Crop Report"
    },
    "repcompare": {
      "newAlias": "Crop Comparison Deterministic vs. Stochastic"
    },
    "repfinance": {
      "newAlias": "Financial Report by Scenario"
    }
  },
  "outputTables": {
    "repfinance": {
      "pivotCols": "rh",
      "class": "display",
      "filter": "none",
      "options": {
        "pageLength": 15,
        "decimals": 3
      },
      "rownames": false
    },
    "repcompare": {
      "class": "display",
      "filter": "none",
      "options": {
        "pageLength": 15,
        "decimals": 3
      },
      "rownames": false
    },
    "repc": {
      "class": "display",
      "filter": "none",
      "options": {
        "pageLength": 15,
        "decimals": 3
      },
      "rownames": false
    }
  },
  "datatable": {
    "class": "stripe",
    "filter": "top",
    "rownames": false,
    "options": {
      "pageLength": 15
    }
  },
  "handsontable": {
    "stretchH": "all",
    "readonly": false,
    "manualColumnResize": true,
    "contextMenu": {
      "enabled": true,
      "allowRowEdit": true,
      "allowColEdit": true
    },
    "height": 700,
    "colWidths": 150
  }
}
