{
  "overwriteSheetOrder": {
    "input": ["_widgets", "iilocdata", "_scalars"],
    "output": ["tourdetails", "_scalars_out"]
  },
  "theme": "browser",
  "defCompMode": "split",
  "roundingDecimals": 2,
  "activateModules": {
    "logFile": true,
    "lstFile": false,
    "loadLocal": true,
    "attachments": true,
    "downloadTempFiles": false
  },
  "autoGenInputGraphs": false,
  "aggregateWidgets": true,
  "storeLogFilesDuration": 7,
  "excelIncludeMeta": true,
  "excelIncludeEmptySheets": true,
  "saveTraceFile": true,
  "pageTitle": "Traveling Salesman",
  "dataRendering": {
    "tourdetails": {
      "outType": "graph",
      "graph": {
        "title": "tourdetails",
        "tool": "leaflet",
        "layersControl": {
          "options": {
            "collapsed": true
          },
          "position": "topright"
        },
        "markers": {
          "1": {
            "lng": "lnga",
            "lat": "lata",
            "iconOptions": {
              "icon": "circle-user",
              "iconColor": "#f3f709",
              "markerColor": "blue"
            },
            "labelOptions": {
              "textsize": "12px",
              "permanent": false
            },
            "label": "[ii]",
            "group": "Cities"
          }
        },
        "flows": {
          "1": {
            "lng0": "lnga",
            "lat0": "lata",
            "lng1": "lngb",
            "lat1": "latb",
            "flow": "flow",
            "color": "#b50c16",
            "minThickness": 5,
            "maxThickness": 5,
            "layerId": "[ii] - [jj]"
          }
        }
      },
      "height": 700
    },
    "iilocdata": {
      "outType": "dtGraph",
      "graph": {
        "title": "",
        "tool": "leaflet",
        "layersControl": {
          "options": {
            "collapsed": true
          },
          "position": "topright"
        },
        "markers": {
          "1": {
            "lng": "lng",
            "lat": "lat",
            "labelOptions": {
              "textsize": "12px",
              "permanent": false
            },
            "label": "[u]",
            "group": "Cities"
          }
        }
      },
      "height": 700
    }
  },
  "datatable": {
    "class": "display",
    "filter": "none",
    "rownames": false,
    "options": {
      "pageLength": 15
    }
  },
  "defaultScenName": "default",
  "inputWidgets": {
    "iilocdata": {
      "widgetType": "custom",
      "rendererName": "mirowidget_iilocdata",
      "apiVersion": 2,
      "alias": "City location information"
    },
    "_scalars": {
      "widgetType": "table",
      "alias": "Advanced Settings",
      "readonly": false,
      "heatmap": false,
      "bigData": false
    }
  },
  "miroLogFile": "tspMiro.log",
  "readme": {
    "tabTitle": "Introduction",
    "filename": "README.md"
  },
  "handsontable": {
    "stretchH": "none",
    "readonly": false,
    "highlightCol": true,
    "highlightRow": true,
    "columnSorting": true,
    "manualColumnResize": true,
    "contextMenu": {
      "enabled": true,
      "allowRowEdit": true,
      "allowColEdit": true,
      "allowReadOnly": false
    },
    "height": 700,
    "colWidths": 150
  }
}
