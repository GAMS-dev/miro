{
    "activateModules": {
        "attachments": true,
        "downloadTempFiles": true,
        "hcube": true,
        "loadLocal": true,
        "logFile": true,
        "lstFile": true
    },
    "aggregateWidgets": true,
    "autoGenInputGraphs": true,
    "customCss": false,
    "dataRendering": {
        "abserror": {
            "graph": {
                "alpha": 0.6,
                "barmode": "overlay",
                "histnorm": "percent",
                "nbins": 20,
                "title": "Absolute Error: Dow Jones vs. Index Fund",
                "tool": "plotly",
                "type": "hist",
                "xaxis": {
                    "title": "Absolute Error"
                },
                "xdata": {
                    "absolute error test": {
                        "alpha": 0.6,
                        "color": "red",
                        "labels": "test"
                    },
                    "absolute error train": {
                        "alpha": 0.6,
                        "color": "darkgreen",
                        "labels": "training"
                    }
                },
                "yaxis": {
                    "title": "Frequency"
                }
            },
            "height": 700,
            "outType": "dtGraph"
        },
        "dowvsindex": {
            "graph": {
                "dyEvent": {
                    "lastdaytraining": {
                        "color": "red",
                        "label": "End of training phase",
                        "labelLoc": "bottom"
                    }
                },
                "dyHighlight": {
                    "hideOnMouseOut": true,
                    "highlightSeriesOpts": {
                        "highlightCircleSize": 5,
                        "strokeBorderWidth": 1,
                        "strokeWidth": 3
                    }
                },
                "dyLegend": {
                    "hideOnMouseOut": true,
                    "show": "always",
                    "width": 450
                },
                "dyOptions": {
                    "drawGrid": true,
                    "drawPoints": false,
                    "fillAlpha": 0.15,
                    "fillGraph": false,
                    "includeZero": false,
                    "logscale": false,
                    "pointSize": 2
                },
                "dyRangeSelector": {
                    "height": 20,
                    "strokeColor": ""
                },
                "title": "Dow Jones vs. Index Fund",
                "tool": "dygraphs",
                "xaxis": {
                    "label": "Date",
                    "name": "x"
                },
                "xdata": "date",
                "yaxis": {
                    "label": "Price",
                    "name": "y"
                },
                "ydata": {
                    "dj": {
                        "drawpoints": true,
                        "fillGraph": false,
                        "label": "Dow Jones index",
                        "stemPlot": false,
                        "stepPlot": false
                    },
                    "index fund": {
                        "drawpoints": true,
                        "fillGraph": false,
                        "label": "Index fund",
                        "stemPlot": false,
                        "stepPlot": false
                    }
                }
            },
            "height": 700,
            "outType": "graph"
        },
        "price": {
            "graph": {
                "color": "symbol",
                "dyHighlight": {
                    "hideOnMouseOut": true,
                    "highlightCircleSize": 3,
                    "highlightSeriesBackgroundAlpha": 0.8,
                    "highlightSeriesOpts": {
                        "strokeBorderColor": "#ffffff",
                        "strokeBorderWidth": null,
                        "strokeWidth": 3
                    }
                },
                "dyLegend": {
                    "labelsSeparateLines": false,
                    "show": "auto",
                    "showZeroValues": false,
                    "width": 250
                },
                "dyOptions": {
                    "drawPoints": false,
                    "fillAlpha": 0.15,
                    "fillGraph": false,
                    "logscale": false,
                    "pointShape": "dot",
                    "pointSize": 2,
                    "stemPlot": false,
                    "stepPlot": false
                },
                "dyRangeSelector": {
                    "fillColor": "#a7b1c4",
                    "height": 25,
                    "keepMouseZoom": true,
                    "retainDateWindow": false,
                    "strokeColor": "#808fab"
                },
                "title": "Stock prices over time (year: 2016)",
                "tool": "dygraphs",
                "xaxis": {
                    "axisLabelFontSize": 14,
                    "axisLineWidth": 0.3,
                    "drawGrid": true,
                    "gridLineWidth": 0.3,
                    "label": "Date",
                    "name": "x"
                },
                "xdata": "date",
                "yaxis": {
                    "axisLabelFontSize": 14,
                    "axisLineWidth": 0.3,
                    "gridLineColor": "gray",
                    "gridLineWidth": 0.3,
                    "independentTicks": true,
                    "label": "Price",
                    "name": "y",
                    "valueRange": [
                        null,
                        null
                    ]
                },
                "yaxis2": {
                    "axisLabelFontSize": 14,
                    "axisLineWidth": 0.3,
                    "drawGrid": true,
                    "gridLineWidth": 0.3,
                    "independentTicks": true,
                    "label": "Price",
                    "name": "y2",
                    "valueRange": [
                        null,
                        null
                    ]
                },
                "ydata": {
                    "value": {
                        "drawPoints": false,
                        "fillGraph": false,
                        "label": "Price",
                        "pointShape": "dot",
                        "pointSize": 2,
                        "stemPlot": false,
                        "stepPlot": false,
                        "yaxis": "y"
                    }
                }
            },
            "height": 700,
            "outType": "graph"
        },
        "pricemerge": {
            "graph": {
                "color": "uni",
                "dyEvent": {
                    "lastdaytraining": {
                        "color": "#ff0000",
                        "label": "End of training phase",
                        "labelLoc": "top",
                        "strokePattern": "dashed"
                    }
                },
                "dyHighlight": {
                    "hideOnMouseOut": true,
                    "highlightCircleSize": 0,
                    "highlightSeriesBackgroundAlpha": 1,
                    "highlightSeriesOpts": {
                        "strokeWidth": 0
                    }
                },
                "dyOptions": {
                    "drawPoints": false,
                    "fillAlpha": 0.15,
                    "fillGraph": false,
                    "logscale": false,
                    "pointShape": "dot",
                    "pointSize": 2,
                    "stemPlot": false,
                    "stepPlot": false
                },
                "dyRangeSelector": {
                    "fillColor": "#a7b1c4",
                    "height": 25,
                    "keepMouseZoom": true,
                    "retainDateWindow": false,
                    "strokeColor": "#808fab"
                },
                "filter": {
                    "col": "uni",
                    "date": false,
                    "label": "Select stock symbol",
                    "multiple": true
                },
                "title": "Price (stocks & dow jones)",
                "tool": "dygraphs",
                "xaxis": {
                    "axisLabelFontSize": 14,
                    "axisLineWidth": 0.3,
                    "drawGrid": true,
                    "gridLineWidth": 0.3,
                    "label": "Date",
                    "name": "x"
                },
                "xdata": "date",
                "yaxis": {
                    "axisLabelFontSize": 14,
                    "axisLineWidth": 0.3,
                    "gridLineColor": "gray",
                    "gridLineWidth": 0.3,
                    "independentTicks": true,
                    "label": "Price",
                    "name": "y",
                    "valueRange": [
                        null,
                        null
                    ]
                },
                "yaxis2": {
                    "axisLabelFontSize": 14,
                    "axisLineWidth": 0.3,
                    "drawGrid": true,
                    "gridLineWidth": 0.3,
                    "independentTicks": true,
                    "label": "Price (stocks & dow jones)",
                    "name": "y2",
                    "valueRange": [
                        null,
                        null
                    ]
                },
                "ydata": {
                    "value": {
                        "drawPoints": false,
                        "fillGraph": false,
                        "label": "Price (stocks & dow jones)",
                        "pointShape": "dot",
                        "pointSize": 2,
                        "stemPlot": false,
                        "stepPlot": false,
                        "yaxis": "y"
                    }
                }
            },
            "height": 700,
            "outType": "graph"
        },
        "stock_weight": {
            "graph": {
                "showlegend": true,
                "title": "Part of portfolio",
                "tool": "plotly",
                "traces": [
                    {
                        "hole": 0,
                        "labels": "symbol",
                        "name": "weight",
                        "values": "value"
                    }
                ],
                "type": "pie"
            },
            "height": 700,
            "outType": "dtGraph"
        }
    },
    "datatable": {
        "class": "stripe",
        "filter": "bottom",
        "options": {
            "pageLength": 15
        },
        "rownames": false
    },
    "defaultRendererOutput": "miroPivot",
    "defaultScenName": "default",
    "defCompMode": "pivot",
    "excelIncludeEmptySheets": true,
    "excelIncludeMeta": true,
    "handsontable": {
        "colWidths": 200,
        "contextMenu": {
            "allowColEdit": false,
            "allowRowEdit": true,
            "enabled": true
        },
        "height": 700,
        "readonly": false,
        "stretchH": "none"
    },
    "hiddenOutputScalars": [
        "firstdaytraining",
        "lastdaytraining"
    ],
    "inputWidgetGroups": [
        {
            "members": "solver",
            "name": "Advanced options"
        }
    ],
    "inputWidgets": {
        "maxstock": {
            "alias": "maximum number of stocks to select",
            "default": 3,
            "label": "select the maximum number of stocks",
            "max": "card(price$symbol)",
            "min": 1,
            "noHcube": false,
            "step": 1,
            "ticks": false,
            "widgetType": "slider"
        },
        "price": {
            "readonlyCols": "date",
            "widgetType": "table"
        },
        "solver": {
            "alias": "MIP-Solver",
            "choices": [
                "BARON",
                "CBC",
                "CPLEX",
                "LINDO",
                "LINDOGLOBAL",
                "MOSEK",
                "SCIP",
                "XPRESS",
                "HIGHS"
            ],
            "label": "Solver to use",
            "selected": "CPLEX",
            "widgetType": "dropdown"
        },
        "trainingdays": {
            "alias": "number of days for training",
            "default": 99,
            "label": "select the number of days for training",
            "max": "card(price$date)",
            "min": 1,
            "step": 1,
            "widgetType": "slider"
        }
    },
    "layoutSettings": {
        "maxTabsExpandedInput": 5,
        "maxTabsExpandedOutput": 5,
        "maxTabsExpandedPivotComp": 5,
        "maxTabsExpandedSplitComp": 5,
        "maxTabsExpandedTabComp": 5
    },
    "miroLogFile": "miro.log",
    "outputGroups": [
        {
            "members": [
                "dowvsindex",
                "abserror"
            ],
            "name": "Dow Jones vs. Index Fund ⨠"
        }
    ],
    "overwriteAliases": {
        "_scalars_out": {
            "newAlias": "Output Scalars"
        },
        "abserror": {
            "newAlias": "absolute error"
        },
        "dowvsindex": {
            "newAlias": "dow jones vs. index fund"
        },
        "error_ratio": {
            "newAlias": "Ratio between error test and error train"
        },
        "error_test": {
            "newAlias": "Absolute error in entire testing phase"
        },
        "error_train": {
            "newAlias": "Absolute error in entire training phase"
        },
        "firstdaytraining": {
            "newAlias": "first date of training period"
        },
        "lastdaytraining": {
            "newAlias": "last date of training period"
        },
        "maxstock": {
            "newAlias": "maximum number of stocks to select"
        },
        "price": {
            "newAlias": "Price"
        },
        "pricemerge": {
            "newAlias": "Price (stocks & dow jones)"
        },
        "solver": {
            "newAlias": "solver"
        },
        "stock_weight": {
            "newAlias": "weight"
        },
        "trainingdays": {
            "newAlias": "number of days for training"
        }
    },
    "overwriteSheetOrder": {
        "input": [
            "_widgets",
            "price",
            "_widgets1"
        ],
        "output": [
            "dowvsindex",
            "stock_weight",
            "abserror",
            "pricemerge",
            "_scalars_out"
        ]
    },
    "pageTitle": "Stock Selection Optimization",
    "pivotCompSettings": {
        "fixedColumns": true,
        "hidePivotControls": false
    },
    "readme": {
        "enableMath": true,
        "filename": "README.md",
        "tabTitle": "Readme"
    },
    "roundingDecimals": 2,
    "saveTraceFile": true,
    "scripts": {
        "hcube": [
            {
                "args": [
                    "nbconvert",
                    "--to",
                    "html",
                    "--execute",
                    "hcube_analysis.ipynb"
                ],
                "command": "jupyter",
                "id": "script1",
                "outputFile": "hcube_analysis.html",
                "title": "High level scalar analysis"
            }
        ]
    },
    "storeLogFilesDuration": 7,
    "symbolLinks": [
        {
            "source": "pricemerge",
            "target": "price"
        }
    ],
    "theme": "browser",
    "UILogo": "pickstock.png"
}
