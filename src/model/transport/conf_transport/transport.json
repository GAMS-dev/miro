{
    "activateModules": {
        "attachments": true,
        "downloadTempFiles": true,
        "hcube": true,
        "loadLocal": true,
        "logFile": true,
        "lstFile": true
    },
    "aggregateWidgets": true,
    "autoGenInputGraphs": true,
    "customCss": false,
    "dataRendering": {
        "_scalars_out": {
            "options": {
                "color": "red",
                "icon": {
                    "lib": "font-awesome",
                    "name": "dollar-sign"
                },
                "width": 4
            },
            "outType": "valueBox"
        },
        "schedule": {
            "graph": {
                "flows": {
                    "1": {
                        "color": "#000000",
                        "flow": "quantities",
                        "lat0": "latp",
                        "lat1": "latm",
                        "layerId": "From [i] to [j]",
                        "lng0": "lngp",
                        "lng1": "lngm",
                        "maxThickness": 10,
                        "minThickness": 0
                    }
                },
                "layersControl": {
                    "options": {
                        "collapsed": true
                    },
                    "overlayGroups": [
                        "Canning plants",
                        "Markets"
                    ],
                    "position": "topright"
                },
                "markers": {
                    "1": {
                        "group": "Canning plants",
                        "iconOptions": {
                            "icon": "gear",
                            "iconColor": "#000000",
                            "markerColor": "red"
                        },
                        "label": "[i] (capacity: [cap])",
                        "labelOptions": {
                            "permanent": false,
                            "style": {
                                "color": "#ff0000"
                            },
                            "textsize": "16px"
                        },
                        "lat": "latp",
                        "lng": "lngp"
                    },
                    "2": {
                        "group": "Markets",
                        "iconOptions": {
                            "icon": "circle-user",
                            "iconColor": "#000000",
                            "markerColor": "blue"
                        },
                        "label": "[j] (demand: [demand])",
                        "labelOptions": {
                            "permanent": false,
                            "style": {
                                "color": "#08629d"
                            },
                            "textsize": "16px"
                        },
                        "lat": "latm",
                        "lng": "lngm"
                    }
                },
                "title": "shipment quantities in cases",
                "tool": "leaflet"
            },
            "height": 700,
            "outType": "graph"
        }
    },
    "datatable": {
        "class": "display",
        "filter": "none",
        "options": {
            "pageLength": 15
        },
        "rownames": false
    },
    "defaultRendererOutput": "miroPivot",
    "defaultScenName": "default",
    "defCompMode": "split",
    "excelIncludeEmptySheets": true,
    "excelIncludeMeta": true,
    "handsontable": {
        "colWidths": 150,
        "contextMenu": {
            "allowColEdit": true,
            "allowRowEdit": true,
            "enabled": true
        },
        "height": 700,
        "readonly": false,
        "stretchH": "none"
    },
    "inputGroups": [
        {
            "members": [
                "ilocdata",
                "jlocdata"
            ],
            "name": "Location information"
        }
    ],
    "inputWidgets": {
        "beta": {
            "alias": "beta (MINLP-only)",
            "default": 0.95,
            "label": "beta (MINLP-only)",
            "max": 1,
            "min": 0,
            "step": 0.01,
            "widgetType": "slider"
        },
        "d": {
            "alias": "distance in thousands of miles",
            "bigData": false,
            "heatmap": false,
            "pivotCols": "i",
            "readonly": false,
            "widgetType": "table"
        },
        "f": {
            "alias": "freight in dollars per case per thousand miles",
            "default": 100,
            "label": "freight in dollars per case per thousand miles",
            "max": 500,
            "min": 1,
            "noHcube": false,
            "step": 1,
            "ticks": true,
            "widgetType": "slider"
        },
        "mins": {
            "alias": "minimum shipment (MIP- and MINLP-only)",
            "default": 100,
            "label": "minimum shipment (MIP- and MINLP-only)",
            "max": 500,
            "min": 0,
            "minStep": 0,
            "noHcube": false,
            "step": 1,
            "ticks": true,
            "widgetType": "slider"
        },
        "type": {
            "alias": "model type",
            "aliases": [
                "LP",
                "MIP",
                "MINLP"
            ],
            "choices": [
                "lp",
                "mip",
                "minlp"
            ],
            "label": "Select the model type",
            "multiple": false,
            "noHcube": false,
            "selected": "lp",
            "widgetType": "dropdown"
        }
    },
    "layoutSettings": {
        "maxTabsExpandedInput": 5,
        "maxTabsExpandedOutput": 5,
        "maxTabsExpandedPivotComp": 5,
        "maxTabsExpandedSplitComp": 5,
        "maxTabsExpandedTabComp": 5
    },
    "miroLogFile": "miro.log",
    "overwriteAliases": {
        "_scalars_out": {
            "newAlias": "Cost"
        },
        "a": {
            "newAlias": "Capacity"
        },
        "b": {
            "newAlias": "Demand"
        },
        "beta": {
            "newAlias": "beta (MINLP-only)"
        },
        "d": {
            "newAlias": "Distance"
        },
        "f": {
            "newAlias": "freight in dollars per case per thousand miles"
        },
        "ilocdata": {
            "newAlias": "Plant location"
        },
        "jlocdata": {
            "newAlias": "Market location"
        },
        "mins": {
            "newAlias": "minimum shipment (MIP- and MINLP-only)"
        },
        "schedule": {
            "newAlias": "Shipment quantities"
        },
        "total_cost": {
            "newAlias": "total transportation costs in thousands of dollars"
        },
        "type": {
            "newAlias": "selected model type"
        }
    },
    "overwriteSheetOrder": {
        "input": [
            "_widgets",
            "a",
            "b",
            "d",
            "ilocdata",
            "jlocdata"
        ],
        "output": [
            "schedule",
            "_scalars_out"
        ]
    },
    "pageTitle": "A transportation problem",
    "pivotCompSettings": {
        "fixedColumns": true,
        "hidePivotControls": false
    },
    "readme": {
        "enableMath": false,
        "filename": "README.md",
        "tabTitle": "Readme"
    },
    "roundingDecimals": 2,
    "saveTraceFile": true,
    "storeLogFilesDuration": 0,
    "theme": "browser"
}
