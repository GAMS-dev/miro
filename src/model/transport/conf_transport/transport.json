{
  "miroLogFile": "miro.log",
  "inputWidgets": {
    "f": {
      "widgetType": "slider",
      "alias": "freight in dollars per case per thousand miles",
      "min": 1,
      "max": 500,
      "default": 100,
      "step": 1,
      "ticks": true,
      "noHcube": false,
      "label": "freight in dollars per case per thousand miles"
    },
    "mins": {
      "widgetType": "slider",
      "alias": "minimum shipment (MIP- and MINLP-only)",
      "min": 0,
      "max": 500,
      "default": 100,
      "step": 1,
      "ticks": true,
      "noHcube": false,
      "minStep": 0,
      "label": "minimum shipment (MIP- and MINLP-only)"
    },
    "beta": {
      "widgetType": "slider",
      "alias": "beta (MINLP-only)",
      "label": "beta (MINLP-only)",
      "min": 0,
      "max": 1,
      "default": 0.95,
      "step": 0.01
    },
    "d": {
      "widgetType": "table",
      "alias": "distance in thousands of miles",
      "readonly": false,
      "heatmap": false,
      "bigData": false,
      "pivotCols": "i"
    },
    "type": {
      "widgetType": "dropdown",
      "alias": "model type",
      "selected": "lp",
      "noHcube": false,
      "multiple": false,
      "label": "Select the model type",
      "choices": ["lp", "mip", "minlp"],
      "aliases": ["LP", "MIP", "MINLP"]
    }
  },
  "dataRendering": {
    "schedule": {
      "outType": "graph",
      "graph": {
        "title": "shipment quantities in cases",
        "tool": "leaflet",
        "layersControl": {
          "options": {
            "collapsed": true
          },
          "position": "topright",
          "overlayGroups": ["Canning plants", "Markets"]
        },
        "markers": {
          "1": {
            "lng": "lngp",
            "lat": "latp",
            "iconOptions": {
              "icon": "cog",
              "iconColor": "#000000",
              "markerColor": "red"
            },
            "labelOptions": {
              "textsize": "16px",
              "permanent": false,
              "style": {
                "color": "#ff0000"
              }
            },
            "label": "[i] (capacity: [cap])",
            "group": "Canning plants"
          },
          "2": {
            "lng": "lngm",
            "lat": "latm",
            "iconOptions": {
              "icon": "user-circle",
              "iconColor": "#000000",
              "markerColor": "blue"
            },
            "labelOptions": {
              "textsize": "16px",
              "permanent": false,
              "style": {
                "color": "#08629d"
              }
            },
            "label": "[j] (demand: [demand])",
            "group": "Markets"
          }
        },
        "flows": {
          "1": {
            "lng0": "lngp",
            "lat0": "latp",
            "lng1": "lngm",
            "lat1": "latm",
            "flow": "quantities",
            "color": "#000000",
            "minThickness": 0,
            "maxThickness": 10,
            "layerId": "From [i] to [j]"
          }
        }
      },
      "height": 700
    },
    "_scalars_out": {
      "outType": "valueBox",
      "options": {
        "width": 4,
        "color": "red",
        "icon": {
          "name": "dollar-sign",
          "lib": "font-awesome"
        }
      }
    }
  },
  "pivottable": {
    "bgColor": "rgb(255,128,0)"
  },
  "theme": "browser",
  "defCompMode": "split",
  "roundingDecimals": 2,
  "activateModules": {
    "logFile": true,
    "lstFile": true,
    "loadLocal": true,
    "attachments": true,
    "downloadTempFiles": true,
    "hcube": true
  },
  "autoGenInputGraphs": true,
  "aggregateWidgets": true,
  "storeLogFilesDuration": 0,
  "excelIncludeMeta": true,
  "excelIncludeEmptySheets": true,
  "saveTraceFile": true,
  "datatable": {
    "class": "display",
    "filter": "none",
    "rownames": false,
    "options": {
      "pageLength": 15
    }
  },
  "overwriteSheetOrder": {
    "input": ["_widgets", "a", "b", "d", "ilocdata", "jlocdata"],
    "output": ["schedule", "_scalars_out"]
  },
  "pageTitle": "A transportation problem",
  "defaultScenName": "default",
  "overwriteAliases": {
    "a": {
      "newAlias": "Capacity"
    },
    "b": {
      "newAlias": "Demand"
    },
    "d": {
      "newAlias": "Distance"
    },
    "ilocdata": {
      "newAlias": "Plant location"
    },
    "jlocdata": {
      "newAlias": "Market location"
    },
    "schedule": {
      "newAlias": "Shipment quantities"
    },
    "_scalars_out": {
      "newAlias": "Cost"
    }
  },
  "inputGroups": [
    {
      "name": "Location information",
      "members": ["ilocdata", "jlocdata"]
    }
  ],
  "readme": {
    "tabTitle": "Readme",
    "filename": "Readme.md"
  },
  "handsontable": {
    "stretchH": "none",
    "readonly": false,
    "highlightCol": true,
    "highlightRow": true,
    "columnSorting": true,
    "manualColumnResize": true,
    "contextMenu": {
      "enabled": true,
      "allowRowEdit": true,
      "allowColEdit": true,
      "allowReadOnly": false
    },
    "height": 700,
    "colWidths": 150
  }
}
