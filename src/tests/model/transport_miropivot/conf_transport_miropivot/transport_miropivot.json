{
    "activateModules": {
        "attachments": true,
        "downloadTempFiles": true,
        "hcube": false,
        "loadLocal": true,
        "logFile": true,
        "lstFile": true,
        "remoteExecution": false
    },
    "aggregateWidgets": true,
    "autoGenInputGraphs": true,
    "customCss": false,
    "dataRendering": {
        "_scalars_out": {
            "options": {
                "color": "red",
                "icon": {
                    "lib": "font-awesome",
                    "name": "dollar-sign"
                },
                "width": 4
            },
            "outType": "valueBox"
        },
        "schedule": {
            "height": 700,
            "options": {
                "cols": {
                    "j": ""
                },
                "filter": {
                    "Hdr": "quantities"
                },
                "pivotRenderer": "bar",
                "rows": "i"
            },
            "outType": "miroPivot"
        },
        "schedule2": {
            "height": 700,
            "options": {
                "cols": {
                    "j": null
                },
                "externalDefaultView": "test123",
                "filter": {
                    "Hdr": "quantities"
                },
                "pivotRenderer": "bar",
                "rows": "i"
            },
            "outType": "miroPivot"
        }
    },
    "datatable": {
        "class": "display",
        "filter": "none",
        "options": {
            "pageLength": 15
        },
        "rownames": false
    },
    "defaultRendererOutput": "miroPivot",
    "defaultScenName": "default",
    "defCompMode": "split",
    "excelIncludeEmptySheets": true,
    "excelIncludeMeta": true,
    "handsontable": {
        "colWidths": 150,
        "contextMenu": {
            "allowColEdit": true,
            "allowRowEdit": true,
            "enabled": true
        },
        "height": 700,
        "readonly": false,
        "stretchH": "none"
    },
    "hiddenOutputSymbols": "_scalars_out",
    "inputGroups": [
        {
            "members": [
                "ilocdata",
                "jlocdata"
            ],
            "name": "Location information",
            "sameTab": false
        }
    ],
    "inputWidgets": {
        "beta": {
            "alias": "beta (MINLP-only)",
            "default": 0.95,
            "label": "beta (MINLP-only)",
            "max": 1,
            "min": 0,
            "step": 0.01,
            "widgetType": "slider"
        },
        "d": {
            "options": {
                "cols": {
                    "j": null
                }
            },
            "tableType": "pivot",
            "widgetType": "table"
        },
        "f": {
            "alias": "freight in dollars per case per thousand miles",
            "default": 100,
            "label": "freight in dollars per case per thousand miles",
            "max": 500,
            "min": 1,
            "noHcube": false,
            "step": 1,
            "ticks": true,
            "widgetType": "slider"
        },
        "mins": {
            "alias": "minimum shipment (MIP- and MINLP-only)",
            "default": 100,
            "label": "minimum shipment (MIP- and MINLP-only)",
            "max": 500,
            "min": 0,
            "minStep": 0,
            "noHcube": false,
            "step": 1,
            "ticks": true,
            "widgetType": "slider"
        },
        "type": {
            "alias": "model type",
            "aliases": [
                "LP",
                "MIP",
                "MINLP"
            ],
            "choices": [
                "lp",
                "mip",
                "minlp"
            ],
            "label": "Select the model type",
            "multiple": false,
            "noHcube": false,
            "selected": "lp",
            "widgetType": "dropdown"
        }
    },
    "layoutSettings": {
        "maxTabsExpandedInput": 5,
        "maxTabsExpandedOutput": 5,
        "maxTabsExpandedPivotComp": 5,
        "maxTabsExpandedSplitComp": 5,
        "maxTabsExpandedTabComp": 5
    },
    "miroLogFile": "miro.log",
    "outputGroups": [
        {
            "members": [
                "schedule",
                "_scalars_out"
            ],
            "name": "Schedule",
            "sameTab": false
        }
    ],
    "overwriteAliases": {
        "_scalars_out": {
            "newAlias": "Cost"
        },
        "a": {
            "newAlias": "Capacity"
        },
        "b": {
            "newAlias": "Demand"
        },
        "beta": {
            "newAlias": "beta (MINLP-only)"
        },
        "d": {
            "newAlias": "Distance"
        },
        "f": {
            "newAlias": "freight in dollars per case per thousand miles"
        },
        "ilocdata": {
            "newAlias": "Plant location"
        },
        "jlocdata": {
            "newAlias": "Market location"
        },
        "mins": {
            "newAlias": "minimum shipment (MIP- and MINLP-only)"
        },
        "schedule": {
            "newAlias": "Shipment quantities"
        },
        "schedule2": {
            "newAlias": "shipment quantities in cases"
        },
        "total_cost": {
            "newAlias": "total transportation costs in thousands of dollars"
        },
        "type": {
            "newAlias": "selected model type"
        }
    },
    "overwriteSheetOrder": {
        "input": [
            "_widgets",
            "a",
            "b",
            "d",
            "ilocdata",
            "jlocdata"
        ],
        "output": [
            "schedule",
            "_scalars_out",
            "schedule2"
        ]
    },
    "pageTitle": "A transportation problem",
    "pivotCompSettings": {
        "emptyUEL": "-",
        "fixedColumns": true,
        "hideEmptyCols": false,
        "hidePivotControls": false,
        "tableSummarySettings": {
            "colEnabled": false,
            "colSummaryFunction": "sum",
            "rowEnabled": false,
            "rowSummaryFunction": "sum"
        }
    },
    "roundingDecimals": 2,
    "saveTraceFile": true,
    "storeLogFilesDuration": 0,
    "theme": "browser"
}
