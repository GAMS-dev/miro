{
  "miroLogFile": "miro.log",
  "pivotCompSettings":{
    "enableHideEmptyCols": true
  },
  "defaultScenName": "default",
  "inputWidgets": {
    "f": {
      "widgetType": "slider",
      "alias": "freight in dollars per case per thousand miles",
      "min": 1,
      "max": 500,
      "default": 100,
      "step": 1,
      "ticks": true,
      "noHcube": false,
      "label": "freight in dollars per case per thousand miles"
    },
    "mins": {
      "widgetType": "slider",
      "alias": "minimum shipment (MIP- and MINLP-only)",
      "min": 0,
      "max": 500,
      "default": 100,
      "step": 1,
      "ticks": true,
      "noHcube": false,
      "minStep": 0,
      "label": "minimum shipment (MIP- and MINLP-only)"
    },
    "beta": {
      "widgetType": "slider",
      "alias": "beta (MINLP-only)",
      "label": "beta (MINLP-only)",
      "min": 0,
      "max": 1,
      "default": 0.95,
      "step": 0.01
    },
    "d": {
      "widgetType": "table",
      "alias": "distance in thousands of miles",
      "readonly": false,
      "heatmap": false,
      "bigData": false,
      "pivotCols": "i"
    },
    "type": {
      "widgetType": "dropdown",
      "alias": "model type",
      "selected": "lp",
      "noHcube": false,
      "multiple": false,
      "label": "Select the model type",
      "choices": ["lp", "mip", "minlp"],
      "aliases": ["LP", "MIP", "MINLP"]
    }
  },
  "dataRendering": {
    "schedule": {
      "outType": "graph",
      "graph": {
        "title": "",
        "tool": "leaflet",
        "layersControl": {
          "options": {
            "collapsed": true
          },
          "position": "topright"
        },
        "markers": {
          "1": {
            "lng": "lngp",
            "lat": "latp",
            "labelOptions": {
              "textsize": "12px",
              "permanent": true,
              "style": {
                "color": "#000000",
                "background-color": "#f58300"
              }
            },
            "label": "[i] (capacity: [cap])"
          },
          "2": {
            "lng": "lngm",
            "lat": "latm",
            "labelOptions": {
              "textsize": "12px",
              "permanent": true
            },
            "label": "[j] (demand: [demand])"
          }
        },
        "flows": {
          "1": {
            "lng0": "lngp",
            "lat0": "latp",
            "lng1": "lngm",
            "lat1": "latm",
            "flow": "quantities",
            "color": "#261a1a",
            "minThickness": 1,
            "maxThickness": 13,
            "layerId": "From [i] to [j]"
          }
        }
      },
      "height": 700
    },
    "_scalars_out": {
      "outType": "valueBox",
      "options": {
        "color": "aqua"
      }
    }
  },
  "pivottable": {
    "bgColor": "rgb(255,128,0)"
  },
  "theme": "browser",
  "defCompMode": "split",
  "roundingDecimals": 2,
  "activateModules": {
    "logFile": true,
    "lstFile": true,
    "loadLocal": true,
    "attachments": true,
    "remoteExecution": false,
    "downloadTempFiles": true
  },
  "autoGenInputGraphs": true,
  "aggregateWidgets": true,
  "storeLogFilesDuration": 7,
  "excelIncludeMeta": true,
  "excelIncludeEmptySheets": true,
  "saveTraceFile": true,
  "datatable": {
    "class": "display",
    "filter": "none",
    "rownames": false,
    "options": {
      "pageLength": 15
    }
  },
  "overwriteSheetOrder": {
    "input": ["a", "b", "d", "ilocdata", "jlocdata", "_widgets"],
    "output": ["schedule", "_scalars_out"]
  },
  "pageTitle": "A transportation problem",
  "inputGroups": [
    {
      "name": "location information",
      "members": ["ilocdata", "jlocdata"]
    }
  ],
  "handsontable": {
    "stretchH": "none",
    "readonly": false,
    "highlightCol": true,
    "highlightRow": true,
    "columnSorting": true,
    "manualColumnResize": true,
    "contextMenu": {
      "enabled": true,
      "allowRowEdit": true,
      "allowColEdit": true,
      "allowReadOnly": false
    },
    "height": 700,
    "colWidths": 150
  }
}
