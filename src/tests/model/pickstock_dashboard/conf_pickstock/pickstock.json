  "theme": "browser",
  "defCompMode": "split",
  "datatable": {
    "class": "display",
    "filter": "none",
    "rownames": false,
    "options": {
      "pageLength": 15
    }
  },
  "symbolLinks": [
    {
      "source": "pricemerge",
      "target": "price"
    }
  ],
  "overwriteSheetOrder": {
    "input": ["price", "_widgets"],
    "output": ["stock_weight", "_scalars_out", "dowvsindex", "abserror", "pricemerge"]
  },
  "hiddenOutputScalars": ["firstdaytraining", "lastdaytraining"],
  "readme": {
    "tabTitle": "Readme",
    "filename": "Readme.md",
    "enableMath": false
  },
  "customCss": false,
  "layoutSettings": {
    "maxTabsExpandedInput": 5,
    "maxTabsExpandedOutput": 5,
    "maxTabsExpandedPivotComp": 5,
    "maxTabsExpandedSplitComp": 5,
    "maxTabsExpandedTabComp": 5
  },
  "defaultRendererOutput": "miroPivot",
  "pivotCompSettings": {
    "hidePivotControls": false,
    "fixedColumns": true
  },
  "overwriteAliases": {
    "maxstock": {
      "newAlias": "maximum number of stocks to select"
    },
    "trainingdays": {
      "newAlias": "number of days for training"
    },
    "error_train": {
      "newAlias": "Absolute error in entire training phase"
    },
    "error_test": {
      "newAlias": "Absolute error in entire testing phase"
    },
    "error_ratio": {
      "newAlias": "Ratio between error test and error train"
    },
    "firstdaytraining": {
      "newAlias": "first date of training period"
    },
    "lastdaytraining": {
      "newAlias": "last date of training period"
    },
    "price": {
      "newAlias": "Price"
    },
    "_scalars_out": {
      "newAlias": "Output Scalars"
    },
    "stock_weight": {
      "newAlias": "weight"
    },
    "dowvsindex": {
      "newAlias": "dow jones vs. index fund"
    },
    "abserror": {
      "newAlias": "absolute error"
    },
    "pricemerge": {
      "newAlias": "Price (stocks & dow jones)"
    }
  },
  "defaultScenName": "default",
  "outputGroups": [
    {
      "name": "Dow Jones vs. Index Fund ðŸ™€",
      "members": ["dowvsindex", "abserror"]
    }
  ],
  "handsontable": {
    "stretchH": "all",
    "readonly": false,
    "contextMenu": {
      "enabled": true,
      "allowRowEdit": true,
      "allowColEdit": true
    },
    "height": 700,
    "colWidths": 300
  }
}
