{
  "UILogo": "pickstock.png",
  "miroLogFile": "miro.log",
  "defaultScenName": "default",
  "storeLogFilesDuration": 7,
  "roundingDecimals": 2,
  "activateModules": {
    "loadLocal": true,
    "logFile": true,
    "lstFile": true,
    "attachments": true,
    "downloadTempFiles": false
  },
  "aggregateWidgets": true,
  "excelIncludeMeta": true,
  "excelIncludeEmptySheets": true,
  "saveTraceFile": true,
  "autoGenInputGraphs": true,
  "theme": "browser",
  "defCompMode": "split",
  "datatable": {
    "class": "display",
    "filter": "none",
    "rownames": false,
    "options": {
      "pageLength": 15
    }
  },
  "handsontable": {
    "stretchH": "none",
    "readonly": false,
    "highlightCol": true,
    "highlightRow": true,
    "columnSorting": true,
    "manualColumnResize": true,
    "contextMenu": {
      "enabled": true,
      "allowRowEdit": true,
      "allowColEdit": true,
      "allowReadOnly": false
    },
    "height": 700,
    "colWidths": 300
  },
  "outputGroups": [
    {
      "name": "Dow Jones vs. Index Fund ðŸ™€",
      "members": ["dowvsindex", "abserror"]
    },
    {
      "name": "test",
      "members": ["_scalars_out", "pricemerge"],
      "sameTab": true
    }
  ],
  "dataRendering":{
    "pricemerge": {
      "outType": "graph",
      "graph": {
        "tool": "dygraphs",
        "xdata": "date",
        "title": "Price (stocks & dow jones)",
        "dyOptions": {
          "logscale": false,
          "stepPlot": false,
          "stemPlot": false,
          "fillGraph": false,
          "fillAlpha": 0.15,
          "drawPoints": false,
          "pointShape": "dot",
          "pointSize": 2
        },
        "xaxis": {
          "name": "x",
          "label": "Date",
          "axisLineWidth": 0.3,
          "axisLabelFontSize": 14,
          "gridLineWidth": 0.3,
          "drawGrid": true
        },
        "yaxis": {
          "name": "y",
          "label": "Price",
          "axisLineWidth": 0.3,
          "axisLabelFontSize": 14,
          "gridLineWidth": 0.3,
          "gridLineColor": "gray",
          "valueRange": [null, null],
          "independentTicks": true
        },
        "yaxis2": {
          "name": "y2",
          "label": "Price (stocks & dow jones)",
          "axisLineWidth": 0.3,
          "axisLabelFontSize": 14,
          "gridLineWidth": 0.3,
          "drawGrid": true,
          "valueRange": [null, null],
          "independentTicks": true
        },
        "ydata": {
          "value": {
            "label": "Price (stocks & dow jones)",
            "stemPlot": false,
            "stepPlot": false,
            "fillGraph": false,
            "drawPoints": false,
            "pointShape": "dot",
            "pointSize": 2,
            "yaxis": "y"
          }
        },
        "dyHighlight": {
          "hideOnMouseOut": true,
          "highlightCircleSize": 0,
          "highlightSeriesBackgroundAlpha": 1,
          "highlightSeriesOpts": {
            "strokeWidth": 0
          }
        },
        "color": "uni",
        "filter": {
          "col": "uni",
          "label": "Select stock symbol",
          "multiple": true,
          "date": false
        },
        "dyEvent": {
          "lastdaytraining": {
            "labelLoc": "top",
            "strokePattern": "dashed",
            "label": "End of training phase",
            "color": "#ff0000"
          }
        },
        "dyRangeSelector": {
          "height": 25,
          "strokeColor": "#808fab",
          "fillColor": "#a7b1c4",
          "retainDateWindow": false,
          "keepMouseZoom": true
        }
      },
      "height": 700
    },
    "_scalars_out": {
      "outType": "valueBox",
      "options": [
        {
          "error_train": {
            "description": "Absolute error in entire training phase",
            "color": "green",
            "icon": {
              "name": "_",
              "lib": "font-awesome"
            },
            "round": 4
          },
          "error_test": {
            "description": "asd",
            "color": "yellow",
            "icon": {
              "name": "_",
              "lib": "font-awesome"
            },
            "round": 4
          }
        },
        {
          "error_ratio": {
            "description": "",
            "color": "red",
            "icon": {
              "name": "_",
              "lib": "font-awesome"
            },
            "round": 1
          }
        },
        {
          "firstdaytraining": {
            "description": "first date of training period",
            "color": "aqua",
            "icon": {
              "name": "circle-play",
              "lib": "font-awesome"
            },
            "round": 0
          }
        }
      ]
    }
  }
}
