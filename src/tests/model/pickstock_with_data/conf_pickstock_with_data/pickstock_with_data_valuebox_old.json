{
  "UILogo": "pickstock.png",
  "miroLogFile": "miro.log",
  "defaultScenName": "default",
  "pivottable": {
    "bgColor": "rgb(255,128,0)"
  },
  "storeLogFilesDuration": 7,
  "roundingDecimals": 2,
  "activateModules": {
    "loadLocal": true,
    "logFile": true,
    "lstFile": true,
    "attachments": true,
    "downloadTempFiles": false
  },
  "aggregateWidgets": true,
  "excelIncludeMeta": true,
  "excelIncludeEmptySheets": true,
  "saveTraceFile": true,
  "autoGenInputGraphs": true,
  "theme": "browser",
  "defCompMode": "split",
  "datatable": {
    "class": "display",
    "filter": "none",
    "rownames": false,
    "options": {
      "pageLength": 15
    }
  },
  "handsontable": {
    "stretchH": "none",
    "readonly": false,
    "highlightCol": true,
    "highlightRow": true,
    "columnSorting": true,
    "manualColumnResize": true,
    "contextMenu": {
      "enabled": true,
      "allowRowEdit": true,
      "allowColEdit": true,
      "allowReadOnly": false
    },
    "height": 700,
    "colWidths": 300
  },
  "outputGroups": [
    {
      "name": "Dow Jones vs. Index Fund ðŸ™€",
      "members": ["dowvsindex", "abserror"]
    }
  ],
  "dataRendering":{
    "_scalars_out": {
      "outType": "valueBox",
      "options": {
        "width": 6,
        "color": "olive",
        "icon": {
          "name": "euro-sign",
          "lib": "font-awesome"
        }
      }
    }
  }
}
